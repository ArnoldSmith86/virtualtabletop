  "plus7": {
    "type": "button",
    "id": "plus7",
    "x": 45,
    "y": 700,
    "width": 50,
    "height": 50,
    "z": 25,
    "clickRoutine": [
      {
        "func": "FOREACH",
        "in": "${PROPERTY loopArray}",
        "loopRoutine": [
          {
            "func": "SELECT",
            "property": "parent",
            "value": "${value}"
          },
          {
            "func": "COUNT"
          },
          {
            "func": "GET",
            "property": "id",
            "variable": "childID"
          },
          {
            "func": "IF",
            "operand1": "${COUNT}",
            "relation": ">",
            "operand2": 0,
            "thenRoutine": [
              {
                "func": "SET",
                "collection": "thisButton",
                "property": "emptyStack",
                "value": false
              }
            ]
          }
        ]
      },
      {
        "func": "IF",
        "operand1": "${PROPERTY emptyStack}",
        "operand2": false,
        "elseRoutine": [
          {
            "func": "MOVE",
            "from": "stock",
            "to": "${PROPERTY loopArray}"
          },
          {
            "func": "FLIP",
            "face": 1,
            "holder": "${PROPERTY loopArray}"
          }
        ]
      },
      {
        "func": "SET",
        "collection": "thisButton",
        "property": "emptyStack",
        "value": null
      }
    ],
    "css": "font-size: 30px; ",
    "loopArray": [
      "reserve1",
      "reserve2",
      "reserve3",
      "reserve4",
      "reserve5",
      "reserve6",
      "reserve7"
    ],
    "text": "+7"
  }
