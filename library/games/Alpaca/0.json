  "autoScore": {
    "type": "button",
    "id": "autoScore",
    "x": 1485,
    "y": 600,
    "layer": -2,
    "z": 4085,
    "movableInEdit": false,
    "css": "border-radius: 15px;font-size:400%",
    "image": "/assets/-331942285_643",
    "clickRoutine": [
      {
        "func": "INPUT",
        "header": "Ending round...",
        "fields": [
          {
            "type": "text",
            "text": "You are about to end the round and calculate scores. Are you sure?"
          }
        ],
        "cancelButtonText": "Cancel",
        "confirmButtonText": "OK"
      },
      {
        "func": "SET",
        "collection": "thisButton",
        "property": "y",
        "value": 600
      },
      {
        "func": "MOVE",
        "from": "playArea",
        "to": "theDeck",
        "count": 0
      },
      {
        "func": "FLIP",
        "holder": "theDeck",
        "face": 0
      },
      {
        "func": "SELECT",
        "type": "seat",
        "property": "player",
        "relation": "!=",
        "value": ""
      },
      {
        "func": "GET",
        "aggregation": "array",
        "property": "index",
        "variable": "index"
      },
      {
        "func": "FOREACH",
        "in": "${index}",
        "loopRoutine": [
          "var tally = 0",
          {
            "func": "SELECT",
            "type": "seat",
            "property": "index",
            "value": "${value}"
          },
          {
            "func": "GET",
            "property": "player",
            "variable": "pID"
          },
          {
            "func": "SELECT",
            "type": "card",
            "property": "owner",
            "value": "${pID}"
          },
          {
            "func": "SET",
            "property": "parent",
            "value": "temp"
          },
          {
            "func": "MOVE",
            "from": "temp",
            "to": "quit${value}",
            "count": 0
          },
          {
            "func": "FLIP",
            "holder": "quit${value}",
            "face": 1
          },
          {
            "func": "SORT",
            "holder": "quit${value}",
            "key": "sortNum",
            "reverse": true
          },
          {
            "func": "SELECT",
            "type": "card",
            "property": "_ancestor",
            "value": "quit${value}",
            "collection": "theseCards"
          },
          {
            "func": "FOREACH",
            "in": [
              1,
              2,
              3,
              4,
              5,
              6,
              10
            ],
            "loopRoutine": [
              {
                "func": "SELECT",
                "source": "theseCards",
                "property": "sortNum",
                "value": "${value}",
                "max": 1
              },
              {
                "func": "COUNT"
              },
              {
                "func": "IF",
                "operand1": "${COUNT}",
                "operand2": 1,
                "thenRoutine": [
                  "var tally = ${tally} + ${value}"
                ]
              }
            ]
          },
          {
            "func": "LABEL",
            "label": "score${value}",
            "mode": "dec",
            "value": "${tally}"
          },
          {
            "func": "IF",
            "operand1": "${tally}",
            "operand2": 0,
            "thenRoutine": [
              {
                "func": "GET",
                "collection": [
                  "score${value}"
                ],
                "property": "text",
                "variable": "winScore"
              },
              {
                "func": "IF",
                "operand1": "${winScore}",
                "operand2": -9,
                "relation": "<",
                "thenRoutine": [
                  {
                    "func": "LABEL",
                    "label": "score${value}",
                    "mode": "inc",
                    "value": 10
                  }
                ],
                "elseRoutine": [
                  {
                    "func": "IF",
                    "operand1": "${winScore}",
                    "operand2": 0,
                    "relation": "<",
                    "thenRoutine": [
                      {
                        "func": "LABEL",
                        "label": "score${value}",
                        "mode": "inc",
                        "value": 1
                      }
                    ]
                  }
                ]
              }
            ]
          }
        ]
      }
    ]
  },
