  "hand": {
    "type": "holder",
    "id": "hand",
    "x": 744,
    "y": 820,
    "height": 180,
    "z": 62,
    "childrenPerOwner": true,
    "css": "background:transparent;border:transparent",
    "onEnter": {
      "activeFace": 1
    },
    "hAnchorPos": "center",
    "minWidth": 111,
    "rightX": 855,
    "enterRoutine": [
      {
        "func": "CALL",
        "routine": "widthRoutine"
      },
      {
        "func": "GET",
        "collection": "child",
        "property": "id",
        "variable": "id"
      },
      {
        "func": "SET",
        "collection": "child",
        "property": "lastOwner",
        "value": "${PROPERTY owner OF $id}"
      }
    ],
    "leaveRoutine": [
      {
        "func": "CALL",
        "routine": "widthRoutine"
      }
    ],
    "widthRoutine": [
      "var cards = {}",
      {
        "func": "SELECT",
        "value": "${PROPERTY id}"
      },
      {
        "func": "FOREACH",
        "loopRoutine": [
          "var owner = ${PROPERTY owner OF $widgetID}",
          "var cards.$owner = ${cards.$owner} + 1"
        ]
      },
      "var countArray = []",
      {
        "func": "FOREACH",
        "in": "${cards}",
        "loopRoutine": [
          {
            "func": "IF",
            "operand1": "${key}",
            "operand2": "null",
            "elseRoutine": [
              "var value = ${value} - 1",
              "var countArray = push ${value}"
            ]
          }
        ]
      },
      "var countArray = ${countArray} numericSort",
      "var countArrayLen = ${countArray} length",
      "var countArrayLen = ${countArrayLen} - 1",
      "var maxCards = ${countArray} getIndex ${countArrayLen}",
      "var rightX = ${PROPERTY x} + ${PROPERTY width}",
      {
        "func": "SET",
        "collection": "thisButton",
        "property": "rightX",
        "value": "${rightX}"
      },
      {
        "func": "IF",
        "operand1": "${maxCards}",
        "operand2": 0,
        "thenRoutine": [
          {
            "func": "SET",
            "collection": "thisButton",
            "property": "width",
            "value": "${PROPERTY minWidth}"
          },
          "var newWidth = ${PROPERTY minWidth}"
        ],
        "elseRoutine": [
          "var revCount = ${maxCards} - 1",
          "var revCount = ${revCount} * ${PROPERTY stackOffsetX}",
          "var newWidth = ${revCount} + ${PROPERTY minWidth}"
        ]
      },
      {
        "func": "IF",
        "operand1": "${PROPERTY hAnchorPos}",
        "operand2": "left",
        "thenRoutine": [
          "var newX = ${PROPERTY x}"
        ],
        "elseRoutine": [
          {
            "func": "IF",
            "operand1": "${PROPERTY hAnchorPos}",
            "operand2": "right",
            "thenRoutine": [
              {
                "func": "IF",
                "operand1": "${countArrayLen}",
                "operand2": -1,
                "thenRoutine": [
                  "var newX = ${PROPERTY x}"
                ],
                "elseRoutine": [
                  "var newX = ${PROPERTY rightX} - ${newWidth}",
                  {
                    "func": "CALL",
                    "routine": "xShiftRoutine"
                  }
                ]
              }
            ],
            "elseRoutine": [
              {
                "func": "IF",
                "operand1": "${PROPERTY hAnchorPos}",
                "operand2": "center",
                "thenRoutine": [
                  {
                    "func": "IF",
                    "operand1": "${countArrayLen}",
                    "operand2": -1,
                    "thenRoutine": [
                      "var newX = ${PROPERTY x}"
                    ],
                    "elseRoutine": [
                      "var halfWidth = ${newWidth} / 2",
                      "var centerX = ${PROPERTY width} / 2",
                      "var centerX = ${PROPERTY x} + ${centerX}",
                      "var newX = ${centerX} - ${halfWidth}"
                    ]
                  },
                  {
                    "func": "CALL",
                    "routine": "xShiftRoutine"
                  }
                ]
              }
            ]
          }
        ]
      },
      {
        "func": "SET",
        "collection": "thisButton",
        "property": "x",
        "value": "${newX}"
      },
      {
        "func": "SET",
        "collection": "thisButton",
        "property": "width",
        "value": "${newWidth}"
      }
    ],
    "xShiftRoutine": [
      {
        "func": "FOREACH",
        "in": "${cards}",
        "loopRoutine": [
          {
            "func": "IF",
            "operand1": "${key}",
            "operand2": "null",
            "elseRoutine": [
              "var cardCount = ${value} - 1",
              "var stackWidth = ${cardCount} - 1",
              "var stackWidth = ${stackWidth} * ${PROPERTY stackOffsetX}",
              "var stackWidth = ${stackWidth} + ${PROPERTY minWidth}",
              "var edges = ${newWidth} - ${stackWidth}",
              "var shiftX = ${edges} / 2",
              "var rightEdge = ${newWidth} - ${stackWidth}",
              {
                "func": "SELECT",
                "property": "owner",
                "value": "${key}"
              },
              {
                "func": "FOREACH",
                "loopRoutine": [
                  {
                    "func": "GET",
                    "property": "x",
                    "variable": "thisX"
                  },
                  {
                    "func": "GET",
                    "property": "z",
                    "variable": "thisZ"
                  },
                  "var thisZ = ${thisZ} - 1",
                  "var zDiff = ${thisZ} * ${PROPERTY stackOffsetX}",
                  {
                    "func": "IF",
                    "operand1": "${PROPERTY hAnchorPos}",
                    "operand2": "right",
                    "thenRoutine": [
                      "var xDiff = ${thisX} - ${rightEdge}"
                    ],
                    "elseRoutine": [
                      "var xDiff = ${thisX} - ${shiftX}"
                    ]
                  },
                  "var adjustX = ${thisX} - ${xDiff}",
                  "var adjustX = ${adjustX} + ${zDiff}",
                  "var adjustX = ${adjustX} + ${PROPERTY dropOffsetX}",
                  {
                    "func": "SET",
                    "property": "x",
                    "value": "${adjustX}"
                  }
                ]
              }
            ]
          }
        ]
      }
    ],
    "stackOffsetX": 45,
    "movableInEdit": false
  },
