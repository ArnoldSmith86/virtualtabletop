                "func": "FLIP",
                "face": 0
              },
              {
                "func": "SELECT",
                "source": "DEFAULT",
                "property": "cardType",
                "value": "${cardType}"
              },
              {
                "func": "FLIP",
                "face": 1
              },
              {
                "func": "MOVE",
                "from": "holder-zap",
                "to": "holder-discard",
                "count": 0
              },
              {
                "func": "TURN",
                "turnCycle": "forward"
              }
            ]
          }
        ]
      }
