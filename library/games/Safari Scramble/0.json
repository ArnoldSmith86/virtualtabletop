  "button-score": {
    "id": "button-score",
    "type": "button",
    "parent": "round-counter",
    "fixedParent": true,
    "x": -25,
    "y": 40,
    "width": 120,
    "height": 30,
    "movable": false,
    "movableInEdit": false,
    "css": {
      "font-size": "16px"
    },
    "text": "Score Round",
    "clickRoutine": [
      {
        "note": "Unless 1st round hasn't started yet, calculate scores",
        "func": "IF",
        "operand1": "${PROPERTY text OF round-counter}",
        "relation": ">",
        "operand2": 0,
        "thenRoutine": [
          {
            "func": "CALL",
            "widget": "scoreboard",
            "routine": "calculateScoresRoutine"
          }
        ]
      }
    ]
  },
  "button-new-round": {
    "id": "button-new-round",
    "type": "button",
    "parent": "round-counter",
    "fixedParent": true,
    "x": -25,
    "y": 80,
    "width": 120,
    "height": 30,
    "movable": false,
    "movableInEdit": false,
    "css": {
      "font-size": "16px"
    },
    "text": "New Round",
    "clickRoutine": [
      {
        "note": "Unless starting the 1st round, record the scores",
        "func": "IF",
        "operand1": "${PROPERTY text OF round-counter}",
        "relation": ">",
        "operand2": 0,
        "thenRoutine": [
          {
            "func": "CALL",
            "widget": "scoreboard",
            "routine": "calculateScoresRoutine"
          }
        ]
      },
      {
        "note": "Increment the round number",
        "func": "LABEL",
        "label": "round-counter",
        "mode": "inc",
        "value": 1
      },
      {
        "note": "Recall the tiles",
        "func": "RECALL",
        "holder": "holder-elephant"
      },
      {
        "note": "Move the tiles into their respective suit holders",
        "func": "FOREACH",
        "in": [
          "elephant",
          "leopard",
          "lion",
          "rhino",
          "zebra"
        ],
        "loopRoutine": [
          {
            "func": "SELECT",
            "property": "cardType",
            "value": "tile-${value}"
          },
          {
            "func": "SET",
            "property": "parent",
            "value": "holder-${value}"
          }
        ]
      },
      {
        "note": "Recall the cards",
        "func": "RECALL",
        "holder": "holder-cards"
      },
      {
        "note": "Shuffle the cards",
        "func": "SHUFFLE",
        "holder": "holder-cards"
      },
      {
        "note": "Select all player seats",
        "func": "SELECT",
        "property": "type",
        "value": "seat"
      },
      {
        "note": "Filter out empty seats",
        "func": "SELECT",
        "mode": "remove",
        "source": "DEFAULT",
        "property": "player",
        "value": ""
      },
      {
        "note": "Determine the player count",
        "func": "COUNT",
        "collection": "DEFAULT"
      },
      {
        "func": "IF",
        "operand1": "${COUNT}",
        "operand2": 5,
        "thenRoutine": [
          "var handSize = 6"
        ],
        "elseRoutine": [
          {
            "func": "IF",
            "operand1": "${COUNT}",
            "operand2": 4,
            "thenRoutine": [
              "var handSize = 7"
            ],
            "elseRoutine": [
              {
                "func": "IF",
                "operand1": "${COUNT}",
                "operand2": 3,
                "thenRoutine": [
                  "var handSize = 9"
                ],
                "elseRoutine": [
                  "var handSize = 14"
                ]
              }
            ]
          }
        ]
      },
      {
        "note": "Deal players their hands for the new round",
        "func": "FOREACH",
        "collection": "DEFAULT",
        "loopRoutine": [
          {
            "func": "MOVE",
            "from": "holder-cards",
            "to": "${widgetID}",
            "count": "${handSize}"
          }
        ]
      },
      {
        "note": "Sort players' hands by id (effectively by suit, then by value)",
        "func": "SORT",
        "holder": "hand",
        "key": "id"
      }
    ]
  },
