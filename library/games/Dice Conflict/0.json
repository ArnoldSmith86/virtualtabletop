  "rollTimer": {
    "type": "timer",
    "id": "rollTimer",
    "parent": "rollDiceButton",
    "scale": 0,
    "z": 28939,
    "movableInEdit": false,
    "css": {
      "transition": "ease-out 0.2s"
    },
    "milliseconds": 490240,
    "precision": 180,
    "diceContainer": "diceRollerHolder",
    "holderIndex": 24,
    "maxRolls": "1",
    "nextStep": 1,
    "rollsDone": 1,
    "millisecondsChangeRoutine": [
      "var thisDiceContainer = ${PROPERTY diceContainer OF rollTimer}",
      {
        "Note": "If the button is paused, then do nothing.",
        "func": "IF",
        "condition": "${PROPERTY paused}",
        "thenRoutine": [
          {
            "func": "SELECT",
            "property": "parent",
            "value": "${thisDiceContainer}",
            "collection": "diceToRoll"
          },
          {
            "func": "SET",
            "collection": "diceToRoll",
            "property": "css",
            "value": "box-shadow:0 0 5px rgba(0 0 0 / 0.5);border-radius:10px;"
          }
        ],
        "elseRoutine": [
          {
            "Note": "If the timer is not paused, then get all the variables need for the timer to perform its functions.",
            "func": "SELECT",
            "property": "id",
            "relation": "==",
            "value": "rollTimer",
            "collection": "rollTimerWidget"
          },
          {
            "func": "SELECT",
            "property": "parent",
            "value": "${thisDiceContainer}",
            "collection": "diceToRoll"
          },
          {
            "func": "SELECT",
            "property": "id",
            "relation": "==",
            "value": "${thisDiceContainer}",
            "collection": "diceRollerHolder"
          },
          {
            "func": "COUNT",
            "collection": "diceToRoll",
            "variable": "numDiceToRoll"
          },
          "// These try to space the dice evenly across the Dice Container",
          "var diceDistanceOffset = ${numDiceToRoll} + 1",
          "var parentWidth = ${PROPERTY width OF $thisDiceContainer}",
          "var parentHeight = ${PROPERTY height OF $thisDiceContainer}",
          "var parentHeight = ${parentHeight} - 60",
          "var distanceToMove = ${parentWidth} / ${diceDistanceOffset}",
          "var xoffset = ${distanceToMove} - 60",
          {
            "func": "FOREACH",
            "collection": "diceToRoll",
            "loopRoutine": [
              {
                "Note": "Die is given a random number of times to land for each roll if not already set.",
                "func": "IF",
                "operand1": "${PROPERTY rollTimes OF $widgetID}",
                "relation": "==",
                "operand2": "--",
                "thenRoutine": [
                  "var rollThisManyTimes = randInt 2 8",
                  {
                    "func": "SET",
                    "property": "rollTimes",
                    "relation": "=",
                    "value": "${rollThisManyTimes}"
                  },
                  {
                    "func": "SET",
                    "property": "originalRollTimes",
                    "relation": "=",
                    "value": "${rollThisManyTimes}"
                  }
                ]
              },
              {
                "Note": "If the number of remaining rolls is 1 or more, keep rolling.",
                "func": "IF",
                "operand1": "${PROPERTY rollTimes OF $widgetID}",
                "relation": ">",
                "operand2": "0",
                "thenRoutine": [
                  {
                    "func": "SELECT",
                    "property": "id",
                    "relation": "==",
                    "value": "${widgetID}",
                    "collection": "thisDieWidget"
                  },
                  {
                    "func": "SET",
                    "property": "css",
                    "relation": "=",
                    "value": "transition: ease-out 0.2s;box-shadow:0 0 5px rgba(0 0 0 / 0.5);border-radius:10px;",
                    "collection": "thisDieWidget"
                  },
                  "var RANDOMFACE = randInt 0 5",
                  {
                    "func": "SET",
                    "property": "activeFace",
                    "relation": "=",
                    "value": "${RANDOMFACE}",
                    "collection": "thisDieWidget"
                  },
                  "var RANDOMROTATION = randInt -360 360",
                  {
                    "func": "SET",
                    "property": "rotation",
                    "relation": "=",
                    "value": "${RANDOMROTATION}",
                    "collection": "thisDieWidget"
                  },
                  "var RANDOM = randInt 9 11",
                  "var RANDOMSCALE = ${RANDOM} / 10",
                  {
                    "func": "SET",
                    "property": "scale",
                    "relation": "=",
                    "value": "${RANDOMSCALE}",
                    "collection": "thisDieWidget"
                  },
                  "var RANDOM-Y = randInt 30 ${parentHeight}",
                  {
                    "func": "SET",
                    "property": "y",
                    "relation": "=",
                    "value": "${RANDOM-Y}",
                    "collection": "thisDieWidget"
                  },
                  "var RANDOMIZEDOFFSET = randInt -45 45",
                  "var RANDOM-X = ${xoffset} + ${RANDOMIZEDOFFSET}",
                  {
                    "func": "SET",
                    "property": "x",
                    "relation": "=",
                    "value": "${RANDOM-X}",
                    "collection": "thisDieWidget"
                  },
                  "var xoffset = ${xoffset} + ${distanceToMove}",
                  "var rollTimes = ${PROPERTY rollTimes OF $widgetID} - 1",
                  {
                    "func": "SET",
                    "collection": "thisDieWidget",
                    "property": "rollTimes",
                    "relation": "=",
                    "value": "${rollTimes}"
                  },
                  {
                    "func": "IF",
                    "operand1": "${PROPERTY rollTimes OF $widgetID}",
                    "relation": ">=",
                    "operand2": "${PROPERTY maxRolls OF rollTimer}",
                    "thenRoutine": [
                      "var maxRolls = ${PROPERTY rollTimes OF $widgetID}",
                      "var maxRolls = ${maxRolls} + 5",
                      {
                        "func": "SET",
                        "collection": "rollTimerWidget",
                        "property": "maxRolls",
                        "relation": "=",
                        "value": "${maxRolls}"
                      }
                    ]
                  }
                ],
                "elseRoutine": [
                  {
                    "func": "SELECT",
                    "property": "id",
                    "relation": "==",
                    "value": "${widgetID}",
                    "collection": "thisDieWidget"
                  },
                  {
                    "Note": "If no rolls remain, set the scale back to 1.",
                    "func": "SET",
                    "property": "scale",
                    "relation": "=",
                    "value": "1",
                    "collection": "thisDieWidget"
                  },
                  {
                    "func": "SET",
                    "collection": "thisDieWidget",
                    "property": "css",
                    "value": "box-shadow:0 0 5px rgba(0 0 0 / 0.5);border-radius:10px;"
                  },
                  "var xoffset = ${xoffset} + ${distanceToMove}"
                ]
              }
            ]
          },
          "// Extra roll to allow the animations to finish.",
          "var extraRolls = ${PROPERTY maxRolls OF rollTimer} + 1",
          {
            "Note": "If the number of rolls done is greater than or equal to the max number of rolls of any die, we're done. Do some cleanup.",
            "func": "IF",
            "operand1": "${PROPERTY rollsDone OF rollTimer}",
            "relation": ">",
            "operand2": "${extraRolls}",
            "thenRoutine": [
              {
                "func": "SET",
                "collection": "rollTimerWidget",
                "property": "maxRolls",
                "value": "1"
              },
              {
                "func": "SET",
                "collection": "rollTimerWidget",
                "property": "rollsDone",
                "value": "0"
              },
              {
                "func": "SET",
                "collection": "diceRollerHolder",
                "property": "alignChildren",
                "value": false
              },
              {
                "Note": "If no rolls remain, set the scale back to 1.",
                "func": "SET",
                "property": "scale",
                "relation": "=",
                "value": "1",
                "collection": "diceToRoll"
              },
              {
                "func": "SET",
                "collection": "diceToRoll",
                "property": "rollTimes",
                "value": "--"
              },
              {
                "func": "SET",
                "collection": "thisButton",
                "property": "paused",
                "value": true
              },
              {
                "func": "SET",
                "collection": [
                  "rollDiceButton"
                ],
                "property": "clickable",
                "value": true
              }
            ]
          },
          "var rollsDone = ${PROPERTY rollsDone OF rollTimer} + 1",
          {
            "func": "SET",
            "collection": "rollTimerWidget",
            "property": "rollsDone",
            "relation": "=",
            "value": "${rollsDone}"
          }
        ]
      }
    ]
  },
