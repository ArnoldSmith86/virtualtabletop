  "p1Get": {
    "type": "button",
    "id": "p1Get",
    "x": 33,
    "y": 6,
    "movable": true,
    "clickRoutine": [
      {
        "func": "SELECT",
        "property": "id",
        "value": [
          "${PROPERTY parent}",
          "${PROPERTY parent}Counter"
        ],
        "relation": "in"
      },
      {
        "func": "SET",
        "property": "player",
        "value": "${playerName}"
      },
      {
        "func": "SELECT",
        "property": "id",
        "value": "${PROPERTY parent}Label"
      },
      {
        "func": "SET",
        "property": "text",
        "value": "${playerName}"
      },
      {
        "func": "MOVE",
        "from": "${PROPERTY parent}",
        "to": "hand",
        "count": 0
      },
      {
        "func": "SET",
        "property": "owner",
        "value": [],
        "collection": "thisButton"
      }
    ],
    "text": "Get",
    "TYPE": "playerButton",
    "parent": "p1",
    "layer": 2
  },
  "p1Label": {
    "type": "label",
    "id": "p1Label",
    "y": -63,
    "z": 22742,
    "text": "Player 1",
    "width": 146,
    "parent": "p1",
    "fixedParent": true
  },
  "column1Button": {
    "type": "button",
    "id": "column1Button",
    "x": 33,
    "y": 414,
    "z": 19,
    "clickRoutine": [
      {
        "func": "SELECT",
        "type": "holder",
        "property": "player",
        "value": "${playerName}"
      },
      {
        "func": "GET",
        "property": "id"
      },
      {
        "func": "SELECT",
        "value": "${id}",
        "type": "card"
      },
      {
        "func": "GET",
        "property": "value"
      },
      {
        "func": "SELECT",
        "value": "${PROPERTY parent}",
        "sortBy": {
          "key": "z"
        },
        "type": "card"
      },
      {
        "func": "COUNT"
      },
      {
        "func": "GET",
        "aggregation": "sum",
        "property": "points"
      },
      {
        "func": "GET",
        "property": "value",
        "variable": "previousValue",
        "aggregation": "last"
      },
      "var isLower = ${value} < ${previousValue}",
      "var isFull = ${COUNT} == 5",
      "var hasToTake = ${isLower} || ${isFull}",
      {
        "func": "IF",
        "thenRoutine": [
          {
            "func": "MOVE",
            "from": "${PROPERTY parent}",
            "to": "discard",
            "count": 0
          },
          {
            "func": "SELECT",
            "type": "label",
            "property": "player",
            "value": "${playerName}"
          },
          {
            "func": "SET",
            "property": "text",
            "relation": "-",
            "value": "${points}"
          }
        ],
        "condition": "${hasToTake}"
      },
      {
        "func": "MOVE",
        "to": "${PROPERTY parent}",
        "from": "${id}"
      }
    ],
    "parent": "column1",
    "layer": 2
  },
