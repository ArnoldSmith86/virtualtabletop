  "Reset Button": {
    "type": "button",
    "id": "Reset Button",
    "x": 1499,
    "width": 100,
    "height": 32,
    "z": 6678,
    "clickRoutine": [
      {
        "func": "SELECT",
        "type": "seat",
        "property": "player",
        "relation": "!=",
        "value": ""
      },
      {
        "func": "COUNT",
        "variable": "players"
      },
      "var lessThan5 = ${players} < 5",
      "var lessThan4 = ${players} < 4",
      "var lessThan3 = ${players} < 3",
      "var kittens = ${players} - 1",
      {
        "func": "RECALL",
        "holder": "Draw"
      },
      {
        "func": "FLIP",
        "holder": "Draw",
        "face": 0
      },
      {
        "func": "SHUFFLE",
        "holder": "Draw"
      },
      {
        "func": "SELECT",
        "property": "defuse",
        "value": true
      },
      {
        "func": "SET",
        "value": "Temp"
      },
      {
        "func": "SHUFFLE",
        "holder": "Temp"
      },
      {
        "func": "MOVE",
        "from": "Temp",
        "to": [
          "seat1",
          "seat2",
          "seat3",
          "seat4",
          "seat5"
        ]
      },
      {
        "func": "MOVE",
        "from": "Temp",
        "to": "Draw",
        "count": 2
      },
      {
        "func": "MOVE",
        "from": "Temp",
        "to": "Draw",
        "count": 0,
        "skip": "${lessThan4}"
      },
      {
        "func": "SELECT",
        "property": "exploding",
        "value": true
      },
      {
        "func": "SET",
        "value": "Out"
      },
      {
        "func": "SHUFFLE",
        "holder": "Draw"
      },
      {
        "func": "MOVE",
        "from": "Draw",
        "to": [
          "seat1",
          "seat2",
          "seat3",
          "seat4",
          "seat5"
        ],
        "count": 7
      },
      {
        "func": "MOVE",
        "from": "Out",
        "to": "Draw",
        "count": "${kittens}"
      },
      {
        "func": "SHUFFLE",
        "holder": "Draw"
      },
      {
        "func": "TURN",
        "turnCycle": "random"
      }
    ],
    "text": "Reset",
    "y": 3
  },
  "Exploding Kitten Button": {
    "type": "button",
    "id": "Exploding Kitten Button",
    "x": 1104,
    "y": 536,
    "width": 100,
    "height": 60,
    "z": 6687,
    "clickRoutine": [
      {
        "func": "COUNT",
        "holder": "Draw"
      },
      "var COUNT = ${COUNT} + 1",
      {
        "func": "INPUT",
        "header": "Exploding Kitten",
        "fields": [
          {
            "type": "number",
            "label": "At which position should the Exploding Kittens card be in the draw pile (1 is on top and ${COUNT} is on bottom)?",
            "variable": "position",
            "value": 1,
            "min": 1,
            "max": "${COUNT}"
          }
        ]
      },
      "var isFirst = ${position} == 1",
      "var realPosition = ${position} - 1",
      {
        "func": "MOVE",
        "from": "Draw",
        "to": "Temp",
        "skip": "${isFirst}",
        "count": "${realPosition}"
      },
      {
        "func": "MOVE",
        "from": "Discard",
        "to": "Draw",
        "face": 0
      },
      {
        "func": "MOVE",
        "from": "Temp",
        "count": 100,
        "to": "Draw"
      }
    ],
    "text": "Defuse"
  },
