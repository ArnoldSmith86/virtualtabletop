  "Reset Button": {
    "type": "button",
    "id": "Reset Button",
    "x": 160,
    "y": 260,
    "z": 10,
    "text": "Next\nGame",
    "clickRoutine": [
      {
        "func": "INPUT",
        "fields": [
          {
            "type": "title",
            "text": "Resetting board ..."
          },
          {
            "type": "subtitle",
            "text": "Are you sure you want to reset the board and swap colors?"
          },
          {
            "type": "checkbox",
            "label": "Delete pieces that were manually added?",
            "value": "${PROPERTY keep}",
            "variable": "keep"
          }
        ]
      },
      {
        "func": "SET",
        "collection": "thisButton",
        "property": "keep",
        "value": "${keep}"
      },
      "// SWAP PLAYERS",
      "var whitePlayer = ${PROPERTY player OF White Seat}",
      "var whiteColor = ${PROPERTY color OF White Seat}",
      "var whiteScore = ${PROPERTY text OF White Score}",
      {
        "func": "SET",
        "property": "player",
        "value": "${PROPERTY player OF Black Seat}",
        "collection": [
          "White Seat"
        ]
      },
      {
        "func": "SET",
        "property": "color",
        "value": "${PROPERTY color OF Black Seat}",
        "collection": [
          "White Seat"
        ]
      },
      {
        "func": "SET",
        "property": "turn",
        "value": true,
        "collection": [
          "White Seat"
        ]
      },
      {
        "func": "SET",
        "property": "text",
        "value": "${PROPERTY text OF Black Score}",
        "collection": [
          "White Score"
        ]
      },
      {
        "func": "SET",
        "property": "player",
        "value": "${whitePlayer}",
        "collection": [
          "Black Seat"
        ]
      },
      {
        "func": "SET",
        "property": "color",
        "value": "${whiteColor}",
        "collection": [
          "Black Seat"
        ]
      },
      {
        "func": "SET",
        "property": "turn",
        "value": false,
        "collection": [
          "Black Seat"
        ]
      },
      {
        "func": "SET",
        "property": "text",
        "value": "${whiteScore}",
        "collection": [
          "Black Score"
        ]
      },
      "// PIECES",
      {
        "func": "SELECT",
        "property": "home",
        "relation": "!="
      },
      {
        "func": "FOREACH",
        "loopRoutine": [
          {
            "func": "SET",
            "property": "parent",
            "value": "${PROPERTY home OF $widgetID}"
          }
        ]
      },
      {
        "func": "IF",
        "condition": "${keep}",
        "thenRoutine": [
          {
            "func": "SELECT",
            "property": "clonedFrom",
            "relation": "!=",
            "value": null
          },
          {
            "func": "DELETE"
          }
        ]
      },
      "// HIGHLIGHT",
      {
        "func": "SELECT",
        "property": "highlight",
        "relation": "!="
      },
      {
        "func": "SET",
        "property": "highlight"
      }
    ],
    "css": {
      "--wcMain": "#ccc",
      "--wcBorder": "#888",
      "color": "#000"
    },
    "keep": true
  },
