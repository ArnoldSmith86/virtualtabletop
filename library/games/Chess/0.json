{
  "A1": {
    "id": "A1",
    "type": "holder",
    "classes": "transparent",
    "dropTarget": {},
    "dropOffsetX": 0,
    "dropOffsetY": 0,
    "css": "background: #888",
    "width": 100,
    "height": 100,
    "y": 800,
    "x": 400,
    "enterRoutine": [
      "// TAKE",
      {
        "func": "SELECT",
        "value": "${PROPERTY id}"
      },
      {
        "func": "FOREACH",
        "collection": "child",
        "loopRoutine": [
          "var color = ${widgetID} substr 0 5",
          "var childID = ${widgetID}"
        ]
      },
      {
        "func": "IF",
        "operand1": "${color}",
        "operand2": "Black",
        "thenRoutine": [
          "var opponent = 'White'"
        ],
        "elseRoutine": [
          "var opponent = 'Black'"
        ]
      },
      {
        "func": "FOREACH",
        "loopRoutine": [
          {
            "func": "IF",
            "operand1": "${widgetID}",
            "operand2": "${childID}",
            "elseRoutine": [
              {
                "func": "SET",
                "value": "${opponent} Captures"
              }
            ]
          }
        ]
      },
      "// TURN",
      {
        "func": "SELECT",
        "type": "seat",
        "property": "player",
        "relation": "==",
        "value": "${playerName}"
      },
      {
        "func": "SET",
        "property": "turn",
        "value": false
      },
      {
        "func": "SELECT",
        "type": "seat",
        "property": "player",
        "relation": "!=",
        "value": "${playerName}"
      },
      {
        "func": "SET",
        "property": "turn",
        "value": true
      },
      "// HIGHLIGHT",
      {
        "func": "SELECT",
        "property": "highlight",
        "relation": "!="
      },
      {
        "func": "SET",
        "property": "highlight"
      },
      {
        "func": "SET",
        "collection": "child",
        "property": "highlight",
        "value": 1
      }
    ],
    "borderRadius": 0
  },
