  "settings": {
    "type": "button",
    "id": "settings",
    "x": 700,
    "y": 400,
    "width": 150,
    "height": 150,
    "z": 2094,
    "classes": "material-icons",
    "clickRoutine": [
      {
        "func": "SELECT",
        "type": "card",
        "property": "flower",
        "relation": "in",
        "value": "${PROPERTY flowerArray}"
      },
      {
        "func": "SELECT",
        "source": "DEFAULT",
        "property": "number",
        "value": 1
      },
      {
        "func": "INPUT",
        "fields": [
          {
            "type": "title",
            "text": "Game Settings:"
          },
          {
            "type": "subtitle",
            "text": "Select card types you do NOT want to use. Choose 4 types to exclude for 2 players, 2 types for 3 players, and none for 4 players. Press confirm to recall all cards and deal 7 to each seated player."
          },
          {
            "type": "choose",
            "source": "DEFAULT",
            "propertyOverride": {
              "activeFace": 1,
              "enlarge": 3
            },
            "min": 0,
            "max": 4,
            "collection": "unusedCards"
          }
        ],
        "confirmButtonText": "Confirm",
        "cancelButtonText": "Cancel"
      },
      {
        "func": "RECALL",
        "holder": "deckHolder"
      },
      {
        "func": "MOVE",
        "from": "deckHolder",
        "to": "deckHolderOff",
        "count": 0
      },
      "var unusedArray = []",
      {
        "func": "FOREACH",
        "collection": "unusedCards",
        "loopRoutine": [
          {
            "func": "SELECT",
            "property": "id",
            "value": "${widgetID}"
          },
          {
            "func": "GET",
            "property": "flower"
          },
          "var unusedArray = push ${flower}"
        ]
      },
      {
        "func": "SELECT",
        "type": "card",
        "property": "flower",
        "relation": "in",
        "value": "${unusedArray}",
        "collection": "unusedCards"
      },
      {
        "func": "RECALL",
        "holder": "deckHolder",
        "excludeCollection": "unusedCards"
      },
      {
        "func": "SHUFFLE",
        "holder": "deckHolder"
      },
      {
        "func": "FLIP",
        "holder": "deckHolder",
        "face": 0
      },
      {
        "func": "MOVE",
        "from": "deckHolder",
        "to": [
          "seat1",
          "seat2",
          "seat3",
          "seat4"
        ],
        "count": 7
      },
      {
        "func": "SET",
        "collection": "thisButton",
        "property": "y",
        "value": 1100
      }
    ],
    "color": "#8c1226",
    "css": "font-size: 140px;color:#8c1226",
    "backgroundColor": "#FFB9C4",
    "borderColor": "white",
    "borderColorOH": "#FFB9C4",
    "null": true,
    "text": "play_arrow",
