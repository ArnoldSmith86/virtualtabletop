            "operand2": 1,
            "thenRoutine": [
              {
                "func": "MOVE",
                "collection": "DEFAULT",
                "to": "offHolder2"
              }
            ],
            "elseRoutine": [
              {
                "func": "MOVE",
                "collection": "DEFAULT",
                "to": "offHolder1"
              }
            ]
          }
        ]
      }
    ],
