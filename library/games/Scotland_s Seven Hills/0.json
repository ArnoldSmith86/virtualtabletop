  "Reset": {
    "type": "button",
    "id": "Reset",
    "x": 1530,
    "y": 10,
    "width": 60,
    "height": 60,
    "z": 16202,
    "text": "Reset",
    "clickRoutine": [
      {
        "func": "INPUT",
        "header": "Reset the game?",
        "fields": [
          {
            "type": "text",
            "text": "Are you sure you want to reset the entire game?"
          }
        ]
      },
      {
        "func": "RECALL",
        "holder": "redDeckHolder1"
      },
      {
        "func": "FLIP",
        "holder": "redDeckHolder1",
        "face": 0
      },
      {
        "func": "RECALL",
        "holder": "blueDeckHolder1"
      },
      {
        "func": "FLIP",
        "holder": "blueDeckHolder1",
        "face": 0
      },
      {
        "func": "SET",
        "collection": [
          "newGame"
        ],
        "property": "display",
        "value": true
      },
      {
        "func": "RESET",
        "property": "reset"
      },
      {
        "note": "Set each player's point total to 0",
        "func": "FOREACH",
        "range": 2,
        "loopRoutine": [
          {
            "func": "SELECT",
            "property": "playerIndex",
            "value": "${value}"
          },
          {
            "func": "SELECT",
            "property": "id",
            "value": "scoreSeat${value}"
          },
          {
            "func": "SET",
            "property": "text",
            "relation": "=",
            "value": 0
          }
        ]
      },
      {
        "func": "SET",
        "collection": [
          "nextRound"
        ],
        "property": "display",
        "value": false
      },
      {
        "func": "SET",
        "collection": [
          "Reset"
        ],
        "property": "display",
        "value": false
      },
      {
        "func": "SET",
        "collection": [
          "choose"
        ],
        "property": "display",
        "value": true
      }
    ],
    "display": false
  },
