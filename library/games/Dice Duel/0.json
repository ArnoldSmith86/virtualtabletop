  "holder1": {
    "type": "holder",
    "id": "holder1",
    "z": 1,
    "width": 150,
    "height": 150,
    "classes": "transparent",
    "css": {
      "border": "2px solid black"
    },
    "borderRadius": 0,
    "parent": "holderBack",
    "dropTarget": {
      "type": "dice"
    },
    "dropOffsetX": 20,
    "dropOffsetY": 20,
    "row": 1,
    "col": 1,
    "enterRoutine": [
      {
        "func": "GET",
        "collection": "child",
        "property": "value"
      },
      {
        "func": "SET",
        "collection": "thisButton",
        "property": "pips",
        "value": "${value}"
      },
      {
        "func": "CALL",
        "routine": "calcRoutine"
      }
    ],
    "leaveRoutine": [
      {
        "func": "SET",
        "collection": "thisButton",
        "property": "pips",
        "value": 0
      },
      {
        "func": "CALL",
        "routine": "calcRoutine"
      }
    ],
    "calcRoutine": [
      {
        "func": "SELECT",
        "type": "holder",
        "property": "row",
        "value": "${PROPERTY row}"
      },
      {
        "func": "GET",
        "aggregation": "sum",
        "property": "pips"
      },
      {
        "func": "SET",
        "collection": [
          "sumRow${PROPERTY row}"
        ],
        "property": "text",
        "value": "${pips}"
      },
      {
        "func": "SELECT",
        "type": "holder",
        "property": "col",
        "value": "${PROPERTY col}"
      },
      {
        "func": "GET",
        "aggregation": "sum",
        "property": "pips"
      },
      {
        "func": "SET",
        "collection": [
          "sumCol${PROPERTY col}"
        ],
        "property": "text",
        "value": "${pips}"
      }
    ],
    "pips": 0
  },
