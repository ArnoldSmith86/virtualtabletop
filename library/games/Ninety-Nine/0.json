    "cardDefaults": {
      "clickRoutine": [
        {
          "func": "GET",
          "collection": "thisButton",
          "property": "parent",
          "variable": "parentID"
        },
        "var inHand = false",
        {
          "func": "IF",
          "operand1": "${parentID}",
          "relation": "==",
          "operand2": "hand",
          "thenRoutine": [
            "var inHand = true"
          ]
        },
        {
          "func": "IF",
          "operand1": "${parentID}",
          "relation": "==",
          "operand2": "revealedHand",
          "thenRoutine": [
            "var inHand = true"
          ]
        },
        {
          "func": "IF",
          "condition": "inHand",
          "thenRoutine": [
            {
              "func": "SET",
              "collection": "thisButton",
              "property": "owner",
              "value": null,
              "relation": "="
            },
            {
              "func": "SELECT",
              "source": "all",
              "property": "playerHolder",
              "relation": "==",
              "value": "${playerName}"
            },
            {
              "func": "GET",
              "property": "id",
              "aggregation": "first",
              "variable": "holderId"
            },
            {
              "func": "SET",
              "collection": "thisButton",
              "property": "parent",
              "value": "${holderId}",
              "relation": "="
            }
          ],
          "elseRoutine": [
            {
              "func": "FLIP",
              "collection": "thisButton"
            }
          ]
        }
