  "settings": {
    "type": "button",
    "id": "settings",
    "x": 1475,
    "width": 50,
    "height": 50,
    "z": 2096,
    "classes": "material-icons",
    "color": "white",
    "css": "font-size: 34px;line-height:1.3",
    "text": "settings",
    "clickRoutine": [
      {
        "func": "INPUT",
        "header": "Game settings:",
        "fields": [
          {
            "type": "checkbox",
            "label": "Use flowers and seasons?",
            "variable": "flowers",
            "value": "${PROPERTY flowers}"
          },
          {
            "type": "checkbox",
            "label": "Display bonus holders?",
            "variable": "holders",
            "value": "${PROPERTY holders}"
          },
          {
            "type": "checkbox",
            "label": "Use jokers?",
            "variable": "jokers",
            "value": "${PROPERTY jokers}"
          }
        ],
        "confirmButtonText": "Confirm",
        "cancelButtonText": "Cancel"
      },
      {
        "func": "SET",
        "collection": "thisButton",
        "property": "flowers",
        "value": "${flowers}"
      },
      {
        "func": "SET",
        "collection": "thisButton",
        "property": "holders",
        "value": "${holders}"
      },
      {
        "func": "SET",
        "collection": "thisButton",
        "property": "jokers",
        "value": "${jokers}"
      },
      {
        "func": "CALL",
        "routine": "applyOptionsRoutine"
      }
    ],
    "applyOptionsRoutine": [
      {
        "func": "RECALL",
        "holder": "deckHolder"
      },
      {
        "func": "SHUFFLE",
        "holder": "deckHolder"
      },
      {
        "func": "FLIP",
        "holder": "deckHolder",
        "face": 0
      },
      {
        "func": "IF",
        "operand1": "${PROPERTY flowers}",
        "operand2": false,
        "thenRoutine": [
          {
            "func": "SELECT",
            "type": "card",
            "property": "flowers",
            "value": true
          },
          {
            "func": "SET",
            "property": "parent",
            "value": "offBoard"
          }
        ]
      },
      {
        "func": "IF",
        "operand1": "${PROPERTY jokers}",
        "operand2": false,
        "thenRoutine": [
          {
            "func": "SELECT",
            "type": "card",
            "property": "joker",
            "value": true
          },
          {
            "func": "SET",
            "property": "parent",
            "value": "offBoard"
          }
        ]
      },
      {
        "func": "IF",
        "operand1": "${PROPERTY holders}",
        "operand2": false,
        "thenRoutine": [
          {
            "func": "SET",
            "collection": [
              "bonus1",
              "bonus2",
              "bonus3",
              "bonus4"
            ],
            "property": "scale",
            "value": 0
          }
        ],
        "elseRoutine": [
          {
            "func": "SET",
            "collection": [
              "bonus1",
              "bonus2",
              "bonus3",
              "bonus4"
            ],
            "property": "scale",
            "value": null
          }
        ]
      }
    ],
    "flowers": true,
    "holders": false,
    "jokers": false,
    "movableInEdit": false
  },
