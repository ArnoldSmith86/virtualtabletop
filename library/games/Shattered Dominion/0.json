  },
  "Selection 2": {
    "type": "holder",
    "id": "Selection 2",
    "x": 16,
    "inheritFrom": "Selection 1",
    "fillRoutine": [
      {
        "func": "IF",
        "condition": "${players}",
        "thenRoutine": [
          {
            "func": "IF",
            "operand1": "${players}",
            "operand2": 2,
            "thenRoutine": [
              "var cards = 4"
            ],
            "elseRoutine": [
              "var cards = ${players}"
            ]
          },
          {
            "func": "MOVE",
            "from": "Draw",
            "to": "${PROPERTY id}",
            "count": "${cards}",
            "face": 1
          },
          {
            "func": "SORT",
            "holder": "${PROPERTY id}",
            "key": "value",
            "reverse": true
          },
          "var height = ${cards} * 84",
          "var height = ${height} + 4",
          "var drop = ${height} - 84",
          "var y = ${drop} + 20",
          {
            "func": "SELECT",
            "property": "id",
            "value": "Selection 1"
          },
          {
            "func": "SET",
            "property": "height",
            "value": "${height}"
          },
          {
            "func": "SET",
            "property": "dropOffsetY",
            "value": "${drop}"
          },
          {
            "func": "SELECT",
            "property": "id",
            "value": "New Tiles"
          },
          {
            "func": "SET",
            "property": "y",
            "value": "${y}"
          }
        ]
      }
    ],
    "y": 330
  },
  "Player 1 - Seat": {
    "type": "seat",
    "id": "Player 1 - Seat",
    "fixedParent": true,
    "x": -165,
    "y": 80,
    "clickRoutine": [
      {
        "func": "SELECT",
        "property": "parent",
        "relation": "in",
        "value": [
          "Draw",
          "Discard",
          "Selection 1",
          "Selection 2"
        ],
        "type": "card"
      },
