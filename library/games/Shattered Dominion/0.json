        "operand1": "${players}",
        "relation": "<",
        "operand2": 4,
        "thenRoutine": [
          "var cards = ${players} * 12",
          "var cards = 48 - ${cards}",
          {
            "func": "MOVE",
            "from": "${PROPERTY id}",
            "to": "Discard",
            "count": "${cards}"
          }
        ]
      }
    ],
    "css": "background: #00000080; border-color: black"
  },
