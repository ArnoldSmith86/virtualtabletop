        "operand2": 5,
        "thenRoutine": [
          "var loopArray = ${PROPERTY array5}"
        ]
      },
      {
        "func": "FOREACH",
        "in": "${loopArray}",
        "loopRoutine": [
          {
            "func": "SELECT",
            "type": "card",
            "property": "parent",
            "value": "market${value}",
            "collection": "card"
          },
          {
            "func": "SELECT",
            "type": "card",
            "property": "parent",
            "value": "marketVP${value}",
            "collection": "vp"
          },
          "var valueMinus = ${value} - 1",
          {
            "func": "SET",
            "collection": "card",
            "property": "parent",
            "value": "market${valueMinus}"
          },
          {
            "func": "SET",
            "collection": "vp",
            "property": "parent",
            "value": "marketVP${valueMinus}"
          }
        ]
      }
    ],
