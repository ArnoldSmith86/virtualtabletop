  "Stack": {
    "type": "holder",
    "id": "Stack",
    "x": 20,
    "y": 643,
    "inheritFrom": "Draw",
    "z": 21,
    "enterRoutine": [
      {
        "func": "SELECT",
        "value": "${PROPERTY id}",
        "sortBy": {
          "key": "z",
          "reverse": false
        }
      },
      {
        "func": "COUNT"
      },
      {
        "func": "IF",
        "operand1": "${COUNT}",
        "relation": ">",
        "operand2": 1,
        "thenRoutine": [
          {
            "func": "SELECT",
            "source": "DEFAULT",
            "value": "${PROPERTY id}",
            "max": 1
          },
          {
            "func": "SET"
          },
          {
            "func": "SET",
            "property": "movable",
            "value": false
          },
          {
            "func": "SELECT",
            "collection": "CARDS_ON_TABLE"
          },
          {
            "func": "SELECT",
            "source": "CARDS_ON_TABLE",
            "property": "owner",
            "value": null,
            "collection": "CARDS_ON_TABLE"
          },
          {
            "func": "SELECT",
            "source": "CARDS_ON_TABLE",
            "property": "deck",
            "value": "Deck",
            "collection": "CARDS_ON_TABLE",
            "sortBy": {
              "key": "z",
              "reverse": true
            }
          },
          "var i = 0",
          {
            "func": "FOREACH",
            "collection": "CARDS_ON_TABLE",
            "loopRoutine": [
              "var x = ${i} * 100",
              "var x = ${x} + ${PROPERTY x}",
              "var x = ${x} + ${PROPERTY width}",
              "var x = ${x} - 30",
              "var y = ${PROPERTY y} + ${PROPERTY dropOffsetY}",
              "var temp = ${i} ** 2.3",
              "var x = ${x} - ${temp}",
              "var temp = ${i} ** 1.8",
              "var y = ${y} - ${temp}",
              "var scale = ${i} * 0.02",
              "var scale = 0.95 - ${scale}",
              "var i = ${i} + 1",
              {
                "func": "SET",
                "property": "x",
                "value": "${x}"
              },
              {
                "func": "SET",
                "property": "y",
                "value": "${y}"
              },
              {
                "func": "SET",
                "property": "scale",
                "value": "${scale}"
              },
              {
                "func": "SET",
                "property": "layer",
                "value": "-4"
              }
            ]
          }
        ]
      }
    ],
    "onEnter": {
      "activeFace": 1
    }
  },
