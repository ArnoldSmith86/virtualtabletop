  "Player 1 - Join": {
    "type": "button",
    "id": "Player 1 - Join",
    "x": 60,
    "y": -10,
    "clickRoutine": [
      "var playerPrefix = substr ${playerName} 0 5",
      {
        "func": "IF",
        "operand1": "${playerPrefix}",
        "operand2": "Guest",
        "thenRoutine": [
          {
            "func": "INPUT",
            "header": "Who are you?",
            "fields": [
              {
                "type": "string",
                "label": "Name",
                "variable": "playerName",
                "value": "${playerName}"
              }
            ]
          }
        ]
      },
      {
        "func": "SET",
        "collection": "thisButton",
        "property": "owner",
        "value": []
      },
      {
        "func": "LABEL",
        "label": "${PROPERTY parent}",
        "value": "${playerName}"
      },
      {
        "func": "SELECT",
        "property": "text",
        "value": "Join"
      },
      {
        "func": "SELECT",
        "source": "DEFAULT",
        "property": "owner",
        "collection": "inactivePlayers"
      },
      {
        "func": "COUNT",
        "collection": "inactivePlayers"
      },
      "var playerCount = 5 - ${COUNT}",
      "var highestCard = 3 * ${playerCount}",
      {
        "func": "RECALL",
        "holder": "Draw"
      },
      {
        "func": "SHUFFLE",
        "holder": "Draw"
      },
      {
        "func": "FLIP",
        "holder": "Draw",
        "face": 0
      },
      {
        "func": "SELECT",
        "property": "deck",
        "value": "Deck",
        "collection": "Cards"
      },
      {
        "func": "SELECT",
        "source": "Cards",
        "property": "number",
        "relation": ">",
        "value": "${highestCard}",
        "collection": "Unused Cards"
      },
      {
        "func": "SET",
        "collection": "Unused Cards",
        "value": "Discard"
      },
      {
        "func": "FOREACH",
        "loopRoutine": [
          "var parent = ${PROPERTY parent OF $widgetID}",
          "var player = ${PROPERTY text OF $parent}",
          {
            "func": "IF",
            "operand1": "${PROPERTY owner OF $widgetID}",
            "relation": "!=",
            "operand2": null,
            "thenRoutine": [
              {
                "func": "SELECT",
                "value": "Draw",
                "type": "card",
                "max": 15,
                "sortBy": "z"
              },
              {
                "func": "SET",
                "value": "Hand"
              },
              {
                "func": "SET",
                "property": "owner",
                "value": "${player}"
              }
            ]
          }
        ]
      },
      {
        "func": "SORT",
        "holder": "Hand",
        "key": "number"
      },
      {
        "func": "SORT",
        "holder": "Hand",
        "key": "image"
      }
    ],
    "text": "Join",
    "parent": "Player 1 - Label"
  },
