  "hand1": {
    "type": "holder",
    "id": "hand1",
    "x": 50,
    "y": 820,
    "width": 1500,
    "height": 180,
    "css": {
      "background": "#ffffff60"
    },
    "dropOffsetX": 10,
    "dropOffsetY": 14,
    "hidePlayerCursors": true,
    "onEnter": {
      "activeFace": 1
    },
    "onLeave": {
      "activeFace": 0
    },
    "onlyVisibleForSeat": "seat1",
    "enterRoutine": [
      {
        "func": "CALL",
        "routine": "offSetRoutine"
      },
      {
        "func": "SORT",
        "holder": "${PROPERTY id}",
        "key": "rank"
      }
    ],
    "leaveRoutine": [
      {
        "func": "CALL",
        "routine": "offSetRoutine"
      }
    ],
    "offSetRoutine": [
      "var cardWidth = 103",
      "var normalOffset = 105",
      {
        "func": "SELECT",
        "property": "parent",
        "value": "${PROPERTY id}"
      },
      {
        "func": "COUNT"
      },
      {
        "func": "IF",
        "operand1": "${COUNT}",
        "relation": "<=",
        "operand2": 1,
        "thenRoutine": [
          {
            "func": "SET",
            "collection": "thisButton",
            "property": "stackOffsetX",
            "value": "${normalOffset}"
          }
        ],
        "elseRoutine": [
          "var modCount = ${COUNT} - 1",
          "var available = ${PROPERTY width} - ${cardWidth}",
          "var dropOffset = ${PROPERTY dropOffsetX} * 2",
          "var available = ${available} - ${dropOffset}",
          "var perCard = ${available} / ${modCount}",
          "var offset = ${perCard} min ${normalOffset}",
          {
            "func": "SET",
            "collection": "thisButton",
            "property": "stackOffsetX",
            "value": "${offset}"
          }
        ]
      },
      {
        "func": "COUNT",
        "holder": "${PROPERTY id}"
      },
      {
        "func": "SET",
        "collection": [
          "seat${PROPERTY pID}C"
        ],
        "property": "text",
        "value": "${COUNT}"
      }
    ],
    "stackOffsetX": 105,
    "pID": 1
  },
