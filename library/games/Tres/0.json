      "::after": {
        "content": "\"Reset\"",
        "display": "none",
        "position": "absolute",
        "margin-top": "80px",
        "margin-left": "0px",
        "color": "black",
        "font-size": "14px",
        "font-family": "roboto"
      },
      ":hover::after": {
        "display": "block"
      },
      "_, .transition": {
        "transition": "transform 600ms"
      },
      "_, .transition.dragging": {
        "transition": "none"
      }
