  "turn1": {
    "type": "button",
    "id": "turn1",
    "parent": "seat1",
    "x": 160,
    "y": -5,
    "width": 50,
    "height": 50,
    "z": 2,
    "classes": "symbols",
    "color": "white",
    "css": {
      "default": {
        "font-size": "40px",
        "--wcMain": "black",
        "--wcBorder": "white",
        "--wcMainOH": "grey",
        "--wcBorderOH": "black"
      },
      "::after": {
        "content": "\"End turn\"",
        "display": "none",
        "position": "absolute",
        "margin-top": "-90px",
        "color": "black",
        "font-size": "14px",
        "font-family": "Roboto"
      },
      ":hover::after": {
        "display": "block"
      }
    },
    "text": "skip_next",
    "clickRoutine": [
      {
        "func": "TURN"
      },
      {
        "func": "SELECT",
        "type": "card",
        "property": "parent",
        "relation": "==",
        "value": null
      },
      {
        "func": "SELECT",
        "source": "DEFAULT",
        "property": "activeFace",
        "value": 1,
        "collection": "faceUp"
      },
      {
        "func": "SELECT",
        "source": "faceUp",
        "property": "id",
        "relation": "!=",
        "value": null,
        "collection": "table"
      },
      {
        "func": "SELECT",
        "mode": "add",
        "type": "card",
        "property": "parent",
        "relation": "in",
        "value": [
          "holder1",
          "holder2",
          "holder3",
          "holder4",
          "holder5",
          "holder6"
        ],
        "collection": "faceUp"
      },
      {
        "func": "COUNT",
        "collection": "faceUp"
      },
      {
        "func": "IF",
        "operand1": "${COUNT}",
        "operand2": 3,
        "thenRoutine": [
          {
            "func": "GET",
            "collection": "faceUp",
            "property": "rank",
            "aggregation": "array",
            "variable": "array"
          },
          "var result = []",
          {
            "func": "FOREACH",
            "in": "${array}",
            "loopRoutine": [
              "var firstIndex = ${array} indexOf ${value}",
              {
                "func": "IF",
                "operand1": "${key}",
                "relation": "==",
                "operand2": "${firstIndex}",
                "thenRoutine": [
                  "var result = push ${value}"
                ]
              }
            ]
          },
          "var length = ${result} length",
          {
            "func": "IF",
            "operand1": "${length}",
            "operand2": 1,
            "thenRoutine": [
              {
                "func": "MOVE",
                "collection": "faceUp",
                "to": "capture${seatIndex}-1"
              }
            ],
            "elseRoutine": [
              {
                "func": "CALL",
                "routine": "returnRoutine"
              }
            ]
          }
        ],
        "elseRoutine": [
          {
            "func": "CALL",
            "routine": "returnRoutine"
          }
        ]
      }
    ],
    "returnRoutine": [
      {
        "func": "SELECT",
        "type": "card",
        "property": "parent",
        "relation": "==",
        "value": null
      },
      {
        "func": "SET",
        "property": "activeFace",
        "value": 0
      },
      {
        "func": "FOREACH",
        "range": [
          6
        ],
        "loopRoutine": [
          {
            "func": "MOVE",
            "from": "holder${value}",
            "to": "hand${value}",
            "count": "all"
          }
        ]
      }
    ]
  },
