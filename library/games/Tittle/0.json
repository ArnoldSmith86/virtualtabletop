  "markerHolder": {
    "type": "holder",
    "id": "markerHolder",
    "x": 1524,
    "y": 617,
    "width": 68,
    "height": 68,
    "layer": 15,
    "z": 10370,
    "css": "background: #ffffff80",
    "enterRoutine": [
      {
        "func": "SELECT",
        "property": "parent",
        "value": "${thisID}"
      },
      {
        "func": "SELECT",
        "property": "marker",
        "value": true,
        "source": "DEFAULT"
      },
      {
        "func": "COUNT"
      },
      {
        "func": "IF",
        "operand1": 1,
        "relation": "<",
        "operand2": "${COUNT}",
        "thenRoutine": [
          {
            "func": "DELETE",
            "collection": "child"
          }
        ]
      }
    ],
    "dropTarget": {
      "marker": true
    },
    "leaveRoutine": [
      {
        "func": "SELECT",
        "property": "parent",
        "value": "${thisID}"
      },
      {
        "func": "SELECT",
        "property": "marker",
        "value": true,
        "source": "DEFAULT"
      },
      {
        "func": "COUNT"
      },
      {
        "func": "IF",
        "operand1": 0,
        "relation": "==",
        "operand2": "${COUNT}",
        "thenRoutine": [
          {
            "func": "CLONE",
            "source": "child"
          },
          {
            "func": "SET",
            "property": "parent",
            "value": "${thisID}"
          },
          {
            "func": "MOVE",
            "from": "${thisID}",
            "to": "${thisID}"
          }
        ]
      }
    ]
  },
