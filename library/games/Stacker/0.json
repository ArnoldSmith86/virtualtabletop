        "count": "${count}",
        "face": 0
      },
      {
        "func": "FLIP",
        "holder": "${PROPERTY parent}",
        "count": 1
      },
      "var parent = ${PROPERTY parent}",
      "var parentParent = ${PROPERTY parent OF $parent}",
      {
        "func": "LABEL",
        "label": "${parentParent} - Label",
        "value": "${playerName}"
      }
    ],
