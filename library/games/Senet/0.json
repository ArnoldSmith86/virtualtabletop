  "clicker": {
    "id": "clicker",
    "x": 560,
    "y": 701,
    "width": 480,
    "height": 250,
    "z": 1,
    "movable": false,
    "movableInEdit": false,
    "layer": 2,
    "clickRoutine": [
      {
        "func": "FOREACH",
        "in": [
          "stick1",
          "stick2",
          "stick3",
          "stick4"
        ],
        "loopRoutine": [
          "var randomClick = randInt 1 2",
          "var randomRot = randInt 0 359",
          {
            "func": "CLICK",
            "collection": [
              "${value}"
            ],
            "count": "${randomClick}"
          },
          {
            "func": "SET",
            "collection": [
              "${value}"
            ],
            "property": "rotation",
            "value": "${randomRot}"
          }
        ]
      },
      {
        "func": "SELECT",
        "property": "id",
        "relation": "in",
        "value": [
          "stick1",
          "stick2",
          "stick3",
          "stick4"
        ]
      },
      {
        "func": "SELECT",
        "source": "DEFAULT",
        "property": "activeFace",
        "value": 1
      },
      {
        "func": "COUNT"
      },
      {
        "func": "IF",
        "operand1": "${COUNT}",
        "operand2": 0,
        "thenRoutine": [
          {
            "func": "SET",
            "collection": [
              "result"
            ],
            "property": "text",
            "value": "Move 5. Go again."
          }
        ]
      },
      {
        "func": "IF",
        "operand1": "${COUNT}",
        "operand2": 1,
        "thenRoutine": [
          {
            "func": "SET",
            "collection": [
              "result"
            ],
            "property": "text",
            "value": "Move 1. Go again."
          }
        ]
      },
      {
        "func": "IF",
        "operand1": "${COUNT}",
        "operand2": 2,
        "thenRoutine": [
          {
            "func": "SET",
            "collection": [
              "result"
            ],
            "property": "text",
            "value": "Move 2. Pass sticks."
          }
        ]
      },
      {
        "func": "IF",
        "operand1": "${COUNT}",
        "operand2": 3,
        "thenRoutine": [
          {
            "func": "SET",
            "collection": [
              "result"
            ],
            "property": "text",
            "value": "Move 3. Pass sticks."
          }
        ]
      },
      {
        "func": "IF",
        "operand1": "${COUNT}",
        "operand2": 4,
        "thenRoutine": [
          {
            "func": "SET",
            "collection": [
              "result"
            ],
            "property": "text",
            "value": "Move 4. Go again."
          }
        ]
      }
    ],
    "css": {
      "border": "1px solid black",
      "box-shadow": "2px 2px 15px inset",
      "opacity": 0.3
    },
    "borderRadius": 4
  },
