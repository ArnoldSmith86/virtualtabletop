  "clicker": {
    "id": "clicker",
    "x": 560,
    "y": 701,
    "width": 480,
    "height": 250,
    "z": 1,
    "movable": false,
    "movableInEdit": false,
    "layer": 2,
    "clickRoutine": [
      {
        "func": "FOREACH",
        "in": [
          "stick1",
          "stick2",
          "stick3",
          "stick4"
        ],
        "loopRoutine": [
          "var randomTransRot = randInt 10 18",
          "var randomTransRot = ${randomTransRot} / 10",
          "var randomTransFlip = randInt 15 22",
          "var randomTransFlip = ${randomTransFlip} / 10",
          "var randomRot = randInt 0 180",
          "var randomZ = randInt 1 4",
          {
            "func": "CLICK",
            "collection": [
              "${value}"
            ]
          },
          {
            "func": "SET",
            "collection": [
              "${value}"
            ],
            "property": "rotation",
            "value": "${randomRot}"
          },
          {
            "func": "SET",
            "collection": [
              "${value}"
            ],
            "property": "z",
            "value": "${randomZ}"
          },
          {
            "func": "SET",
            "collection": [
              "${value}"
            ],
            "property": "rotTrans",
            "value": "${randomTransRot}"
          },
          {
            "func": "SET",
            "collection": [
              "${value}"
            ],
            "property": "flipTrans",
            "value": "${randomTransFlip}"
          }
        ]
      },
      {
        "func": "SELECT",
        "property": "id",
        "relation": "in",
        "value": [
          "stick1",
          "stick2",
          "stick3",
          "stick4"
        ]
      },
      "var a = ${PROPERTY state} + 1",
      "var b = ${a} % 2",
      {
        "func": "SET",
        "collection": "thisButton",
        "property": "state",
        "value": "${b}"
      },
      {
        "func": "IF",
        "operand1": "${a}",
        "operand2": 1,
        "thenRoutine": [
          {
            "func": "SET",
            "collection": [
              "result1"
            ],
            "property": "scale",
            "value": 1
          },
          {
            "func": "SET",
            "collection": [
              "result1"
            ],
            "property": "speed",
            "value": 0.5
          },
          {
            "func": "SET",
            "collection": [
              "result2"
            ],
            "property": "scale",
            "value": 0
          },
          {
            "func": "SET",
            "collection": [
              "result2"
            ],
            "property": "speed",
            "value": 0
          }
        ],
        "elseRoutine": [
          {
            "func": "SET",
            "collection": [
              "result1"
            ],
            "property": "scale",
            "value": 0
          },
          {
            "func": "SET",
            "collection": [
              "result1"
            ],
            "property": "speed",
            "value": 0
          },
          {
            "func": "SET",
            "collection": [
              "result2"
            ],
            "property": "scale",
            "value": 1
          },
          {
            "func": "SET",
            "collection": [
              "result2"
            ],
            "property": "speed",
            "value": 0.5
          }
        ]
      },
      {
        "func": "SELECT",
        "source": "DEFAULT",
        "property": "activeFace",
        "value": 0
      },
      {
        "func": "COUNT"
      },
      {
        "func": "IF",
        "operand1": "${COUNT}",
        "operand2": 0,
        "thenRoutine": [
          {
            "func": "SET",
            "collection": [
              "result1",
              "result2"
            ],
            "property": "text",
            "value": "Move 5. Go again."
          }
        ]
      },
      {
        "func": "IF",
        "operand1": "${COUNT}",
        "operand2": 1,
        "thenRoutine": [
          {
            "func": "SET",
            "collection": [
              "result1",
              "result2"
            ],
            "property": "text",
            "value": "Move 1. Go again."
          }
        ]
      },
      {
        "func": "IF",
        "operand1": "${COUNT}",
        "operand2": 2,
