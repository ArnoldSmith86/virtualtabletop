    "enterRoutine": [
      {
        "func": "SELECT",
        "property": "parent",
        "value": "${thisID}",
        "sortBy": {
          "key": "z",
          "reverse": false
        }
      },
      {
        "func": "COUNT"
      },
      {
        "func": "IF",
        "operand1": "${COUNT}",
        "relation": ">",
        "operand2": 1,
        "thenRoutine": [
          {
            "func": "GET",
            "property": "z",
            "aggregation": "min",
            "variable": "pieceZ"
          },
          {
            "func": "SELECT",
            "source": "DEFAULT",
            "property": "z",
            "value": "${pieceZ}"
          },
          {
            "func": "GET",
            "property": "_absoluteX",
            "variable": "absX"
          },
          {
            "func": "GET",
            "property": "_absoluteY",
            "variable": "absY"
          },
          {
            "func": "SET",
            "property": "parent",
            "value": null
          },
          {
            "func": "SET",
            "property": "x",
            "value": "${absX}"
          },
          {
            "func": "SET",
            "property": "y",
            "value": "${absY}"
          }
        ]
      }
    ]
