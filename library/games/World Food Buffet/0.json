  "button-new-round": {
    "id": "button-new-round",
    "type": "button",
    "parent": "round-counter",
    "fixedParent": true,
    "x": -25,
    "y": 40,
    "width": 120,
    "height": 30,
    "movable": false,
    "movableInEdit": false,
    "css": {
      "font-size": "16px"
    },
    "text": "New Round",
    "clickRoutine": [
      {
        "note": "Score the round that is finishing",
        "func": "CALL",
        "widget": "scoreboard",
        "routine": "calculateScoresRoutine"
      },
      {
        "note": "Disable scoring during upcoming movements",
        "func": "SET",
        "collection": [
          "scoreboard"
        ],
        "property": "skipScoring",
        "value": true
      },
      {
        "note": "Increment the round number",
        "func": "LABEL",
        "label": "round-counter",
        "mode": "inc",
        "value": 1
      },
      {
        "note": "Move the tiles back to their respective holders",
        "func": "FOREACH",
        "in": [
          "burger",
          "pizza",
          "pretzel",
          "sushi",
          "taco"
        ],
        "loopRoutine": [
          {
            "func": "SELECT",
            "type": "card",
            "property": "cardType",
            "value": "tile-${value}"
          },
          {
            "func": "MOVE",
            "collection": "DEFAULT",
            "to": "holder-${value}"
          }
        ]
      },
      {
        "note": "Recall the cards",
        "func": "RECALL",
        "holder": "holder-cards"
      },
      {
        "note": "Shuffle the cards",
        "func": "SHUFFLE",
        "holder": "holder-cards"
      },
      {
        "note": "Determine the player count",
        "func": "COUNT",
        "collection": "activeSeats"
      },
      {
        "func": "IF",
        "operand1": "${COUNT}",
        "operand2": 5,
        "thenRoutine": [
          "var handSize = 6"
        ],
        "elseRoutine": [
          {
            "func": "IF",
            "operand1": "${COUNT}",
            "operand2": 4,
            "thenRoutine": [
              "var handSize = 7"
            ],
            "elseRoutine": [
              {
                "func": "IF",
                "operand1": "${COUNT}",
                "operand2": 3,
                "thenRoutine": [
                  "var handSize = 9"
                ],
                "elseRoutine": [
                  "var handSize = 14"
                ]
              }
            ]
          }
        ]
      },
      {
        "note": "Deal players their hands for the new round",
        "func": "MOVE",
        "from": "holder-cards",
        "to": "${activeSeats}",
        "count": "${handSize}"
      },
      {
        "note": "Sort players' hands by id (effectively by suit, then by value)",
        "func": "SORT",
        "holder": "hand",
        "key": "id"
      },
      {
        "note": "Re-enable scoring if the game is not already over",
        "func": "IF",
        "operand1": "${PROPERTY text OF round-counter}",
        "relation": "<=",
        "operand2": "${COUNT}",
        "thenRoutine": [
          {
            "func": "SET",
            "collection": [
              "scoreboard"
            ],
            "property": "skipScoring",
            "value": false
          },
          {
            "func": "SCORE",
            "mode": "set",
            "round": "${PROPERTY text OF round-counter}",
            "value": 0
          }
        ]
      }
    ]
  },
