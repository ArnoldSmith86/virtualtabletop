              },
              {
                "func": "SET",
                "collection": [
                  "factoryCount3"
                ],
                "property": "text",
                "value": 10
              },
              {
                "func": "SET",
                "collection": [
                  "factoryCount2"
                ],
                "property": "text",
                "value": 21
              },
              {
                "func": "SET",
                "collection": [
                  "factory3"
                ],
                "property": "display",
                "value": true
              },
              {
                "func": "SET",
                "collection": [
                  "factory4"
                ],
                "property": "display",
                "value": false
              }
            ],
            "elseRoutine": [
              {
                "func": "SET",
