        "font-size": "14px"
      },
      ":hover::after": {
        "display": "block"
      }
    },
    "ignoreZoom": true,
    "text": "restart",
    "clickRoutine": [
      {
        "func": "INPUT",
        "fields": [
          {
            "type": "title",
            "text": "Restarting game ..."
          },
          {
            "type": "subtitle",
            "text": "This will completely reset the board and start a new game. Are you sure?"
          },
          {
            "type": "number",
            "min": 2,
            "max": 4,
            "label": "Choose number of players",
            "value": "${PROPERTY playerCount}",
            "variable": "playerCount"
          }
        ],
        "confirmButtonText": "Yes"
      },
      {
        "func": "RESET"
      },
      "var playerCount = length ${activeSeats}",
      "var 2p = ${playerCount} < 3",
      "var 3p = ${playerCount} == 3",
      "var 4p = ${playerCount} == 4",
      {
        "func": "IF",
        "condition": "${2p}",
        "thenRoutine": [
          {
            "func": "SET",
