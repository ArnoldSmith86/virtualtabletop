  "autoScore": {
    "type": "button",
    "id": "autoScore",
    "x": 1485,
    "y": 1200,
    "layer": -2,
    "z": 4085,
    "css": "border-radius: 15px;font-size:400%",
    "image": "/assets/-331942285_643",
    "clickRoutine": [
      {
        "func": "SET",
        "collection": "thisButton",
        "property": "y",
        "value": 1200
      },
      {
        "func": "FOREACH",
        "in": [
          1,
          2,
          3,
          4,
          5,
          6,
          7
        ],
        "loopRoutine": [
          {
            "func": "SELECT",
            "type": "card",
            "property": "parent",
            "value": "stack${value}"
          },
          {
            "func": "GET",
            "aggregation": "array",
            "property": "sortNum",
            "variable": "pointsArray"
          },
          "var pointsArray = reverse ${pointsArray}",
          "var tally = 0",
          {
            "func": "FOREACH",
            "in": "${pointsArray}",
            "loopRoutine": [
              "var nextKey = ${key} + 1",
              "var diff = ${pointsArray.$key} - ${pointsArray.$nextKey}",
              {
                "func": "IF",
                "operand1": "${diff}",
                "operand2": 1,
                "elseRoutine": [
                  "var tally = ${tally} + ${pointsArray.$key}"
                ]
              }
            ]
          },
          {
            "func": "LABEL",
            "label": [
              "score${value}"
            ],
            "mode": "inc",
            "value": "${tally}"
          }
        ]
      },
      {
        "func": "SELECT",
        "type": "seat",
        "property": "player",
        "relation": "!=",
        "value": null
      },
      {
        "func": "GET",
        "aggregation": "array",
        "property": "player",
        "variable": "player"
      },
      {
        "func": "GET",
        "aggregation": "array",
        "property": "index",
        "variable": "index"
      },
      {
        "func": "FOREACH",
        "in": "${player}",
        "loopRoutine": [
          {
            "func": "SELECT",
            "type": "card",
            "property": "owner",
            "value": "${value}"
          },
          {
            "func": "COUNT"
          },
          {
            "func": "LABEL",
            "label": [
              "score${index.$key}"
            ],
            "mode": "dec",
            "value": "${COUNT}"
          }
        ]
      }
    ],
    "movableInEdit": false
  },
