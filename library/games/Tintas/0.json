  "hexMaster": {
    "type": "holder",
    "id": "hexMaster",
    "x": 174,
    "y": -203,
    "width": 112,
    "height": 112,
    "movableInEdit": false,
    "dropOffsetX": 25,
    "dropOffsetY": 25,
    "dropTarget": {},
    "grid": [
      {
        "x": 150,
        "y": 86.6,
        "offsetX": -68.3,
        "offsetY": -19.6
      },
      {
        "x": 150,
        "y": 86.6,
        "offsetX": 6.7,
        "offsetY": 23.7
      }
    ],
    "enterRoutine": [
      {
        "func": "COUNT",
        "holder": "${PROPERTY id}"
      },
      {
        "func": "GET",
        "collection": "child",
        "property": "id"
      },
      {
        "func": "IF",
        "operand1": "${COUNT}",
        "relation": ">",
        "operand2": 1,
        "thenRoutine": [
          {
            "func": "SELECT",
            "property": "parent",
            "value": "${PROPERTY id}"
          },
          {
            "func": "SELECT",
            "mode": "remove",
            "property": "id",
            "value": "${id}"
          },
          {
            "func": "GET",
            "property": "color"
          },
          {
            "func": "GET",
            "property": "historyColor"
          },
          {
            "func": "GET",
            "property": "colorType"
          },
          {
            "func": "MOVE",
            "to": "holder${seatIndex}-${colorType}"
          },
          {
            "func": "CALL",
            "widget": "historyLabel",
            "routine": "addendumRoutine",
            "arguments": {
              "addendum": "<b>${playerName}</b> captured a <span style='color:${historyColor}'>${historyColor} piece</span>"
            }
          }
        ]
      },
      {
        "func": "IF",
        "condition": "${PROPERTY pawn OF $id}",
        "thenRoutine": [
          {
            "func": "SET",
            "collection": "child",
            "property": "x",
            "value": 32
          },
          {
            "func": "SET",
            "collection": "child",
            "property": "y",
            "value": 13
          }
        ]
      }
    ],
    "css": {
      "default": {
        "background-color": "#e6e1c2",
        "clip-path": "polygon(25% 6.67%, 75% 6.67%, 100% 50%, 75% 93.33%, 25% 93.33%, 0% 50%)"
      }
    }
  },
