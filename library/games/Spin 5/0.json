    "classes": "symbols",
    "css": "font-size: 250%",
    "text": "rotate_ccw",
    "textColor": "black",
    "textColorOH": "pink",
    "clickRoutine": [
      {
        "func": "SELECT",
        "type": "card",
        "property": "parent",
        "relation": "in",
        "value": "${PROPERTY gridGroup}"
      },
      {
        "func": "SET",
        "property": "css",
        "value": "transition:linear 0.65s"
      },
      "var board = ${PROPERTY board}",
      "var rotation = ${PROPERTY rotation OF $board} - 90",
      {
        "func": "SET",
        "collection": [
          "${board}"
        ],
        "property": "rotation",
        "value": "${rotation}"
      },
      {
        "func": "TURN"
      },
      {
        "func": "SELECT",
        "property": "parent",
        "value": "${board}"
      },
      {
        "func": "GET",
        "property": "id",
        "aggregation": "array",
        "variable": "id"
      },
      {
        "func": "SELECT",
        "type": "card",
        "property": "parent",
        "relation": "in",
        "value": "${id}"
      },
      "var sign = ${rotation} sign",
      "var rotationAbs = ${rotation} abs",
      {
        "func": "IF",
        "operand1": "${sign}",
        "operand2": 1,
        "thenRoutine": [
          "var rotationAbs = ${rotationAbs} * -1",
          {
            "func": "SET",
            "property": "rotation",
            "value": "${rotationAbs}"
          }
        ],
        "elseRoutine": [
          {
            "func": "IF",
            "operand1": "${sign}",
            "operand2": -1,
            "thenRoutine": [
              {
                "func": "SET",
                "property": "rotation",
                "value": "${rotationAbs}"
              }
            ],
            "elseRoutine": [
              {
                "func": "SET",
                "property": "rotation",
                "value": 0
              }
            ]
          }
        ]
      }
    ],
