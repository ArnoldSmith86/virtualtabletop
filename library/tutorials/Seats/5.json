  "calls": {
    "type": "button",
    "id": "calls",
    "x": 1487,
    "y": 551,
    "z": 76431,
    "checkSeat1Routine": [
      {
        "note": "There is a separate routine for each board. The button for board 1 calls 2, 3, and 4.  The button for board 2 calls 1, 3, and 4.  Etc."
      },
      {
        "note": "Create a loop that runs through each of the 4 seats.",
        "func": "FOREACH",
        "in": [
          "seat1",
          "seat2",
          "seat3",
          "seat4"
        ],
        "loopRoutine": [
          {
            "note": "Get the value of each item in the array for this board's visibility property."
          },
          "var tempSeatID1 = ${pArray1.$key}",
          {
            "func": "IF",
            "operand1": "${tempSeatID1}",
            "operand2": null,
            "thenRoutine": [
              {
                "note": "If that position in the array is empty/null, then do nothing, otherwise run the elseRoutine"
              }
            ],
            "elseRoutine": [
              {
                "func": "IF",
                "operand1": "${tempSeatID1}",
                "operand2": "${thisSeatID}",
                "thenRoutine": [
                  {
                    "note": "If the player pressing the button appears in the visibility array, then do nothing. What this really means is that the player's name will not be added to the new array being built in the elseRoutine"
                  }
                ],
                "elseRoutine": [
                  {
                    "note": "If the player pressing the button is not in the visibility array, then we want to keep that other player in the visibility array, so assign that player a position in the array."
                  },
                  "var newArray1.$key = ${tempSeatID1}"
                ]
              },
              {
                "note": "Set the array of players -- minus the player pressing the button -- back into the visibility property for this board.",
                "func": "SELECT",
                "property": "id",
                "value": "boardHolder1"
              },
              {
                "func": "SET",
                "property": "onlyVisibleForSeat",
                "value": "${newArray1}"
              }
            ]
          }
        ]
      }
    ],
    "checkSeat2Routine": [
      {
        "func": "FOREACH",
        "in": [
          "seat1",
          "seat2",
          "seat3",
          "seat4"
        ],
        "loopRoutine": [
          "var tempSeatID2 = ${pArray2.$key}",
          {
            "func": "IF",
            "operand1": "${tempSeatID2}",
            "operand2": null,
            "thenRoutine": [],
            "elseRoutine": [
              {
                "func": "IF",
                "operand1": "${tempSeatID2}",
                "operand2": "${thisSeatID}",
                "thenRoutine": [],
                "elseRoutine": [
                  "var newArray2.$key = ${tempSeatID2}"
                ]
              },
              {
                "func": "SELECT",
                "property": "id",
                "value": "boardHolder2"
              },
              {
                "func": "SET",
                "property": "onlyVisibleForSeat",
                "value": "${newArray2}"
              }
            ]
          }
        ]
      }
    ],
    "checkSeat3Routine": [
      {
        "func": "FOREACH",
        "in": [
          "seat1",
          "seat2",
          "seat3",
          "seat4"
        ],
        "loopRoutine": [
          "var tempSeatID3 = ${pArray3.$key}",
          {
            "func": "IF",
            "operand1": "${tempSeatID3}",
            "operand2": null,
            "thenRoutine": [],
            "elseRoutine": [
              {
                "func": "IF",
                "operand1": "${tempSeatID3}",
                "operand2": "${thisSeatID}",
                "thenRoutine": [],
                "elseRoutine": [
                  "var newArray3.$key = ${tempSeatID3}"
                ]
              },
              {
                "func": "SELECT",
                "property": "id",
                "value": "boardHolder3"
              },
              {
                "func": "SET",
                "property": "onlyVisibleForSeat",
                "value": "${newArray3}"
              }
            ]
          }
        ]
      }
    ],
    "checkSeat4Routine": [
      {
        "func": "FOREACH",
        "in": [
          "seat1",
          "seat2",
          "seat3",
          "seat4"
        ],
        "loopRoutine": [
          "var tempSeatID4 = ${pArray4.$key}",
          {
