  "hand": {
    "type": "holder",
    "onEnter": {
      "activeFace": 1
    },
    "onLeave": {
      "activeFace": 0
    },
    "dropOffsetX": 10,
    "dropOffsetY": 14,
    "stackOffsetX": 40,
    "childrenPerOwner": true,
    "x": 50,
    "y": 820,
    "width": 1500,
    "height": 180,
    "id": "hand",
    "enterRoutine": [
      {
        "func": "CALL",
        "routine": "countRoutine"
      }
    ],
    "leaveRoutine": [
      {
        "func": "CALL",
        "routine": "countRoutine"
      }
    ],
    "countRoutine": [
      {
        "note": "Do an iteration for each seat in the room",
        "func": "FOREACH",
        "in": [
          "seat1",
          "seat2",
          "seat3"
        ],
        "loopRoutine": [
          {
            "note": "Get the 'player' property from the seat and put it into the variable 'pID'",
            "func": "GET",
            "collection": [
              "${value}"
            ],
            "property": "player",
            "variable": "pID"
          },
          {
            "note": "Select all the cards in the hand with the owner name matching 'pID'",
            "func": "SELECT",
            "property": "parent",
            "value": "${PROPERTY id}"
          },
          {
            "func": "SELECT",
            "source": "DEFAULT",
            "property": "owner",
            "value": "${pID}",
            "collection": "cardsInHand"
          },
          {
            "note": "Count just those cards",
            "func": "COUNT",
            "collection": "cardsInHand"
          },
          "var holderNum = ${key} + 1 // key starts with 0 so add one to match our holder name",
          "var holderName = 'countSeat' + ${holderNum} // concat the holder name",
          {
            "note": "Set the text value of the holder to the count",
            "func": "LABEL",
            "collection": [
              "${holderName}"
            ],
            "mode": "set",
            "value": "${COUNT}"
          }
        ]
      }
    ]
  },
