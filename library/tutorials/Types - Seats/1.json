{
  "seat1": {
    "type": "seat",
    "id": "seat1",
    "x": 111,
    "y": 182,
    "z": 27,
    "hideWhenUnused": true,
    "turn": true,
    "clickRoutine": [
      "var playerPrefix = substr ${playerName} 0 5",
      {
        "func": "IF",
        "operand1": "${playerPrefix}",
        "operand2": "Guest",
        "thenRoutine": [
          {
            "func": "INPUT",
            "header": "Enter your name and choice of player color.",
            "fields": [
              {
                "type": "string",
                "label": "Name",
                "variable": "playerName",
                "value": "${playerName}"
              },
              {
                "type": "color",
                "label": "Color",
                "variable": "playerColor",
                "value": "${playerColor}"
              }
            ]
          }
        ]
      },
      {
        "func": "CLICK",
        "collection": "thisButton",
        "mode": "ignoreClickRoutine"
      }
    ]
  },
  "seat2": {
    "type": "seat",
    "id": "seat2",
    "x": 512,
    "y": 175,
    "z": 24,
    "index": 2,
    "hideWhenUnused": true,
    "clickRoutine": [
      "var seatStatus = ${PROPERTY occupied}",
      {
        "func": "IF",
        "operand1": "${seatStatus}",
        "operand2": 1,
        "thenRoutine": [
          {
            "func": "INPUT",
            "header": "Getting up ...",
            "fields": [
              {
                "type": "text",
                "text": "Do you want to get up from your seat?"
              }
            ]
          },
          {
            "func": "SET",
            "collection": "thisButton",
            "property": "occupied",
            "value": 0
          },
          {
            "func": "CLICK",
            "collection": "thisButton",
            "mode": "ignoreClickRoutine"
          }
        ],
        "elseRoutine": [
          {
            "func": "INPUT",
            "header": "Enter your name.",
            "fields": [
              {
                "type": "string",
                "label": "Name",
                "variable": "playerName",
                "value": "${playerName}"
              }
            ]
          },
          {
            "func": "SET",
            "collection": "thisButton",
            "property": "occupied",
            "value": 1
          },
          {
            "func": "CLICK",
            "collection": "thisButton",
            "mode": "ignoreClickRoutine"
          },
          {
            "func": "SET",
            "collection": "thisButton",
            "property": "player",
            "value": "${playerName}"
          }
        ]
      }
    ],
    "occupied": 0
  },
  "seat3": {
    "type": "seat",
    "id": "seat3",
    "x": 875,
    "y": 175,
    "z": 25,
    "index": 3,
    "playerChangeRoutine": [
      {
        "func": "IF",
        "operand1": "${PROPERTY text OF labelCount}",
        "operand2": 0,
        "thenRoutine": [
          {
            "func": "LABEL",
            "label": "labelCount",
            "value": 1
          }
        ],
        "elseRoutine": [
          {
            "func": "LABEL",
            "label": "labelCount",
            "value": 0
          }
        ]
      }
    ],
    "hideWhenUnused": true
  },
