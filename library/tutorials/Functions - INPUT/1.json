    "z": 22838,
    "clickRoutine": [
      {
        "func": "SELECT",
        "type": "card",
        "property": "suit",
        "value": "S",
        "collection": "spades"
      },
      {
        "func": "INPUT",
        "fields": [
          {
            "type": "title",
            "text": "Spade cards from the entire room"
          },
          {
            "type": "subtitle",
            "text": "Select 1"
          },
          {
            "type": "choose",
            "source": "spades",
            "variable": "thisVariable",
            "propertyOverride": {
              "activeFace": 1
            },
            "label": "The 6 is pre-selected, but you can change it.",
            "value": "visw_6_S",
            "collection": "thisCollection",
            "scale": 0.8
          }
        ]
      },
      {
        "func": "SET",
        "collection": [
          "variable"
        ],
        "property": "text",
        "value": "${thisVariable}"
      },
      "var array = []",
      {
        "func": "FOREACH",
        "collection": "thisCollection",
        "loopRoutine": [
          {
            "func": "GET",
            "property": "id"
          },
          "var array = ${array} + ${id}"
        ]
      },
      {
        "func": "SET",
        "collection": [
          "collection"
        ],
        "property": "text",
        "value": "${array}"
      }
    ],
    "text": "Collection"
  },
