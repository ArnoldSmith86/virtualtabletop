  "choosebutton1": {
    "type": "button",
    "id": "choosebutton1",
    "x": 85,
    "y": 250,
    "z": 22838,
    "clickRoutine": [
      {
        "func": "INPUT",
        "fields": [
          {
            "type": "title",
            "text": "Cards from demoHolder"
          },
          {
            "type": "subtitle",
            "text": "Select up to 3"
          },
          {
            "type": "choose",
            "holder": [
              "demoHolder"
            ],
            "collection": "thisCollection",
            "variable": "thisVariable",
            "max": 3
          }
        ]
      },
      {
        "func": "SET",
        "collection": [
          "variable"
        ],
        "property": "text",
        "value": "${thisVariable}"
      },
      "var array = []",
      {
        "func": "FOREACH",
        "collection": "thisCollection",
        "loopRoutine": [
          {
            "func": "GET",
            "property": "id"
          },
          "var array = ${array} + ${id}"
        ]
      },
      {
        "func": "SET",
        "collection": [
          "collection"
        ],
        "property": "text",
        "value": "${array}"
      }
    ],
    "text": "Holder"
  },
