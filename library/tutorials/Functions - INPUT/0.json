  "jkmt": {
    "type": "button",
    "clickRoutine": [
      "var inputArray = []",
      {
        "func": "FOREACH",
        "in": "${activePlayers}",
        "loopRoutine": [
          {
            "func": "VAR",
            "variables": {
              "thisEntry": {
                "value": "${value}",
                "text": "${value}"
              }
            }
          },
          "var inputArray = push ${thisEntry}"
        ]
      },
      {
        "func": "FLIP",
        "collection": [
          "checker"
        ]
      },
      {
        "func": "INPUT",
        "fields": [
          {
            "type": "select",
            "label": "Choose a player to see the INPUT checkbox overlay.",
            "variable": "thisPlayer",
            "options": "${inputArray}"
          }
        ]
      },
      {
        "func": "INPUT",
        "player": "${thisPlayer}",
        "fields": [
          {
            "type": "checkbox",
            "label": "Check or uncheck this box",
            "value": true,
            "variable": "checkbox"
          }
        ]
      },
      {
        "func": "SET",
        "collection": "thisButton",
        "property": "text",
        "value": "${thisPlayer} chose ${checkbox}"
      }
    ],
    "x": 95,
    "y": 550,
    "z": 22841,
    "id": "jkmt",
    "text": "Player"
  },
