  "onfg": {
    "type": "button",
    "text": "locked",
    "x": 660,
    "y": 350,
    "z": 5,
    "id": "onfg",
    "clickRoutine": [
      {
        "func": "SET",
        "collection": "thisButton",
        "property": "clickable",
        "relation": "!"
      }
    ],
    "clickableChangeRoutine": [
      {
        "func": "IF",
        "condition": "${PROPERTY clickable}",
        "thenRoutine": [
          {
            "func": "LABEL",
            "collection": "thisButton",
            "value": "unlocked"
          },
          {
            "func": "SET",
            "collection": "thisButton",
            "property": "backgroundColor",
            "value": "green"
          }
        ],
        "elseRoutine": [
          {
            "func": "LABEL",
            "collection": "thisButton",
            "value": "locked"
          },
          {
            "func": "SET",
            "collection": "thisButton",
            "property": "backgroundColor",
            "value": "red"
          }
        ]
      }
    ],
    "backgroundColor": "red",
    "borderColor": "#555",
    "clickable": false
  },
