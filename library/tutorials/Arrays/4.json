  "Word Button": {
    "type": "button",
    "text": "Random Word",
    "clickRoutine": [
      "var words = 'alpha,bravo,charlie,delta,echo,foxtrot,golf,hotel,india,juliet,kilo,lima,mike,november,oscar,papa,quebec,romeo,sierra,tango,uniform,victor,whiskey,x-ray,yankee,zulu' split ',' //Split the list of words into an array using commas as a split point",
      "var wordCount = ${words} length //Calculate the length of (number of words in) the array",
      "var wordCount = ${wordCount} - 1 //Subtract one from the previous value since the array positions start with an index of 0",
      "var RANDOM = randInt 0 ${wordCount} //Calculate a random integer between 0 and the number of words in the array",
      "var word = ${words} getIndex ${RANDOM} //Get the word at the index position of the random number",
      "var word = ${word} toUpperCase //convert the word to uppercase",
      "var target = ${PROPERTY target}",
      {
        "func": "LABEL",
        "label": "${target}",
        "value": "${word}"
      }
    ],
    "x": 717,
    "y": 264,
    "z": 19,
    "id": "Word Button",
    "target": "Word 5-5"
  },
