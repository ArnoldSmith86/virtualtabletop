  "clickme": {
    "type": "button",
    "text": "Click me",
    "clickRoutine": [
      {
        "func": "SELECT",
        "property": "id",
        "value": "${thisID}",
        "collection": "me"
      },
      "var index = ${PROPERTY index}",
      "var array = ${PROPERTY array}",
      "var newIndex = ${index} + 1",
      "var arrayLength = ${array} length",
      "var dontresetIndex = ${newIndex} != ${arrayLength}",
      {
        "func": "IF",
        "condition": "${dontresetIndex}",
        "elseRoutine": [
          "var newIndex = 0"
        ]
      },
      "var holder = ${array} getIndex ${newIndex}",
      {
        "func": "SET",
        "collection": "me",
        "property": "parent",
        "value": "${holder}"
      },
      {
        "func": "SET",
        "collection": "me",
        "property": "index",
        "value": "${newIndex}"
      },
      {
        "func": "SELECT",
        "property": "id",
        "value": "indexText",
        "collection": "indexText"
      },
      {
        "func": "SET",
        "collection": "indexText",
        "property": "text",
        "value": "${newIndex}"
      },
      {
        "func": "SELECT",
        "property": "id",
        "value": "holderText",
        "collection": "holderText"
      },
      {
        "func": "SET",
        "collection": "holderText",
        "property": "text",
        "value": "${holder}"
      }
    ],
    "x": 4,
    "y": 4,
    "z": 26372,
    "id": "clickme",
