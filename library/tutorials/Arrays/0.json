  "n7pi": {
    "type": "button",
    "clickRoutine": [
      "var text = ${PROPERTY text OF splitText}",
      "var splitArray = ${text} split ',' //Recreate what happens in the split button",
      "var arrayValues = ${PROPERTY arrayValues OF frombtn}",
      "var fromArray = from ${arrayValues} //Recreate what happens in the from button",
      "var combinedArray = ${splitArray} concatArray ${fromArray} //Concat the two arrays into one",
      {
        "func": "LABEL",
        "label": "array",
        "value": "${combinedArray}"
      }
    ],
    "x": 733,
    "y": 916,
    "z": 26409,
    "id": "n7pi",
    "text": "concatArray",
    "css": "font-size: 13px; "
