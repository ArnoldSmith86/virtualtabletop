  "qwxy": {
    "type": "label",
    "text": "Each button changes the label text to the name of the button IF they are activated.  Button 1 and button 2 deactivate after one click, but reset each other.  Button 3 does not deactivate or reset either of the other buttons.  Button 1 and 2 have a property 'clicked' (a made-up property) to track if they have been clicked.  Clicking twice on button 1 or 2 without clicking on the other will do nothing, because their effect on the label will be SKIPPED. The background color of the button is changed after clicking to show it is inactive.",
    "x": 118,
    "y": 143,
    "z": 191,
    "id": "qwxy",
    "width": 500,
    "height": 160
  },
  "button1_ID": {
    "type": "button",
    "text": "button 1",
    "clickRoutine": [
      "// Selects button 2, and puts it in myCollectionName",
      {
        "func": "SELECT",
        "source": "all",
        "property": "id",
        "relation": "==",
        "value": "button2_ID",
        "collection": "myCollectionName"
      },
      "// Resets button 2 by setting a property named clicked to false and resetting css",
      {
        "func": "SET",
        "collection": "myCollectionName",
        "property": "clicked",
        "value": false
      },
      {
        "func": "SET",
        "collection": "myCollectionName",
        "property": "css",
        "value": null
      },
      "// Selects the label and puts it in myCollectionName",
      {
        "func": "SELECT",
        "source": "all",
        "property": "id",
        "relation": "==",
        "value": "label_ID",
        "collection": "myCollectionName"
      },
      "// Selects this button and puts it in myCollectionName2",
      {
        "func": "SELECT",
        "source": "all",
        "property": "id",
        "relation": "==",
        "value": "button1_ID",
        "collection": "myCollectionName2"
      },
      "// Gets the value for the property clicked and stores it in clickedStatus",
      {
        "func": "GET",
        "collection": "myCollectionName2",
        "property": "clicked",
        "variable": "clickedStatus"
      },
      "// sets the text of the label to button 1, but is skipped if clicked status = true",
      {
        "func": "IF",
        "operand1": "${clickedStatus}",
        "operand2": "button1",
        "elseRoutine": [
          {
            "func": "SET",
            "collection": "myCollectionName",
            "property": "text",
            "value": "button 1"
          }
        ]
      },
      "// sets the property clicked to true for this button",
      {
        "func": "SET",
        "collection": "thisButton",
        "property": "clicked",
        "value": true
      },
      "// changes the color for this button to show it is not clickable",
      {
        "func": "SET",
        "collection": "thisButton",
        "property": "css",
        "value": "--wcMain:lightblue; --wcMainOH:lightblue"
      }
    ],
    "x": 256,
    "y": 314,
    "z": 36,
    "id": "button1_ID",
