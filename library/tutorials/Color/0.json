        ],
        "property": "cBack",
        "value": "${baseColor1}"
      },
      {
        "func": "SET",
        "collection": [
          "compare1"
        ],
        "property": "cText",
        "value": "${newColor1}"
      },
      "var baseColor2 = ${PROPERTY text OF compare2}",
      "var newColor2 = colorContrast ${baseColor2}",
      {
        "func": "SET",
        "collection": [
          "compare2"
        ],
        "property": "cBack",
        "value": "${baseColor2}"
      },
      {
        "func": "SET",
        "collection": [
          "compare2"
        ],
        "property": "cText",
        "value": "${newColor2}"
      }
    ]
  },
