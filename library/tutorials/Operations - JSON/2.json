        "mode": "reset"
      }
    ],
    "classes": "symbols",
    "css": "font-size: 28px",
    "text": "reload"
  },
  "timer": {
    "type": "timer",
    "id": "timer",
    "x": 1306,
    "y": 934,
    "millisecondsChangeRoutine": [
      {
        "func": "IF",
        "operand1": "${PROPERTY milliseconds}",
        "relation": ">=",
        "operand2": 5000,
        "thenRoutine": [
          {
            "func": "CALL",
            "routine": "spectateRoutine"
          },
          {
            "func": "SET",
            "collection": "thisButton",
            "property": "milliseconds",
            "value": 0
          }
        ]
      }
    ],
    "spectateRoutine": [
      {
        "func": "SELECT",
        "type": "card",
        "property": "id",
        "relation": "!=",
        "value": null,
        "collection": "allCards"
      },
      {
        "func": "SET",
        "collection": "allCards",
        "property": "owner",
        "value": null
      },
      {
        "func": "SET",
        "collection": "allCards",
        "property": "parent",
        "value": "notInHand"
      },
      {
        "func": "SORT",
        "holder": "notInHand",
        "key": "null"
      },
      "var json = fetch 'https://virtualtabletop.io/state/TutorialExampleSpectator'",
      "var parsed = jsonParse ${json} ",
      {
        "func": "FOREACH",
        "range": [
          4
        ],
        "loopRoutine": [
          "var thisSeat = 'seat' + ${value}",
          "var thisHand = 'handP' + ${value}",
          "var watchSeat = ${parsed.$thisSeat}",
          "var watchSeatPlayer = ${watchSeat.player}",
          "var watchSeatColor = ${watchSeat.color}",
          {
            "func": "SET",
            "collection": [
              "${thisSeat}"
            ],
            "property": "player",
            "value": "${watchSeatPlayer}"
          },
          {
            "func": "SET",
            "collection": [
              "${thisSeat}"
            ],
            "property": "color",
            "value": "${watchSeatColor}"
          },
          {
            "func": "FOREACH",
            "collection": "allCards",
            "loopRoutine": [
              "var thisCard = ${parsed.$widgetID}",
              "var thisCardOwner = ${thisCard.owner}",
              "var thisCardParent = ${thisCard.parent}",
              {
                "func": "IF",
                "operand1": "${thisCardOwner}",
                "operand2": "${watchSeatPlayer}",
                "thenRoutine": [
                  {
                    "func": "SET",
                    "collection": [
                      "${widgetID}"
                    ],
                    "property": "parent",
                    "value": "${thisHand}"
                  },
                  {
                    "func": "SET",
                    "collection": [
                      "${widgetID}"
                    ],
                    "property": "owner",
                    "value": null
                  }
                ]
              }
            ]
          }
        ]
      },
      {
        "func": "SELECT",
        "source": "allCards",
        "mode": "intersect",
        "property": "parent",
        "relation": "in",
        "value": [
          "handP1",
          "handP2",
          "handP3",
          "handP4"
        ],
        "collection": "notInHandCards"
      },
      {
        "func": "SET",
        "collection": "notInHandCards",
        "property": "parent",
        "value": "notInHand"
      }
    ]
  },
