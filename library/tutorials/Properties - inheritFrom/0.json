  "slidebuttonx": {
    "type": "button",
    "id": "slidebuttonx",
    "x": 1297,
    "y": 650,
    "z": 1002,
    "movable": true,
    "changeRoutine": [
      "// Use xMin to set the left limit of the travel of the slider.  This will depend on where the slider is on the board.  Use xRange to set how far the slider can travel.  In this case, it is 120 so the scale will max out at 1.2.  Use yLock at the y coordinate on which the slider will travel to prevent it from moving up or down.",
      {
        "Note": "Choose the things you want to change the scale on. For this example, it is the holder containing the chess board and pieces",
        "func": "SELECT",
        "source": "all",
        "property": "id",
        "relation": "==",
        "value": "bottomholder",
        "collection": "turnHolder"
      },
      {
        "Note": "Set the y value so that it will not change.",
        "func": "GET",
        "collection": "thisButton",
        "property": "yLock",
        "variable": "yLock"
      },
      {
        "func": "SET",
        "collection": "thisButton",
        "property": "y",
        "value": "${yLock}"
      },
      {
        "Note": "Determine the x values that will limit control for this slider.",
        "func": "GET",
        "collection": "thisButton",
        "property": "xMin",
        "variable": "xMin"
      },
      {
        "func": "GET",
        "collection": "thisButton",
        "property": "xRange",
        "variable": "xRange"
      },
      "var xMax = ${xRange} + ${xMin}",
      {
        "Note": "Get current x position.",
        "func": "GET",
        "collection": "thisButton",
        "property": "x",
        "variable": "xSlide"
      },
      {
        "Note": "If x goes above the maximum, set x to the maximum.",
        "func": "IF",
        "relation": ">",
        "thenRoutine": [
          {
            "func": "SET",
            "collection": "thisButton",
            "property": "x",
            "relation": "=",
            "value": "${xMax}"
          }
        ],
        "elseRoutine": [
          {
            "Note": "If x goes below the minimum, then set x to the minimum.",
            "func": "IF",
            "relation": "<",
            "thenRoutine": [
              {
                "func": "SET",
                "collection": "thisButton",
                "property": "x",
                "relation": "=",
                "value": "${xMin}"
              }
            ],
            "elseRoutine": [
              "var scaleVar = ${xSlide} - ${xMin} // If x is between the min and the max, calculate the value considering where its starting limit is.",
              "var scaleVar = ${scaleVar} / 100 // For scaling, need to divide the result by 100.",
              {
                "Note": "Set the scale based on the slider position.",
                "func": "SET",
                "collection": "turnHolder",
                "property": "scale",
                "relation": "=",
                "value": "${scaleVar}"
              }
            ],
            "operand1": "${xSlide}",
            "operand2": "${xMin}"
          }
        ],
        "operand1": "${xSlide}",
        "operand2": "${xMax}"
      }
    ],
    "text": "X",
    "xMin": 1100,
    "xRange": 300,
    "yLock": 650
  },
  "slidebuttony": {
    "type": "button",
    "id": "slidebuttony",
    "x": 1500,
    "y": 427,
    "z": 1001,
    "movable": true,
    "changeRoutine": [
      "// Use xLock at the x coordinate on which the slider will travel to prevent it from moving left or right. Use yMin to set the upper limit of the travel of the slider.  This will depend on where the slider is on the board.  Use yRange to set how far the slider can travel.  In this case, it is 360 so the setting will go from 0 to 360.",
      {
        "Note": "Choose the things you want to change.",
        "func": "SELECT",
        "source": "all",
        "property": "id",
        "relation": "==",
        "value": "bottomholder",
        "collection": "turnHolder"
      },
      {
        "func": "SELECT",
        "source": "all",
        "property": "gamepiece",
        "relation": "==",
        "value": true,
        "collection": "staticPiece"
      },
      {
        "Note": "Set the x value so that it will not change.",
        "func": "GET",
        "collection": "thisButton",
        "property": "xLock",
        "variable": "xLock"
      },
      {
        "func": "SET",
        "collection": "thisButton",
        "property": "x",
        "value": "${xLock}"
      },
      {
        "Note": "Determine the y values that will limit control for this slider.",
        "func": "GET",
        "collection": "thisButton",
        "property": "yMin",
        "variable": "yMin"
      },
      {
        "func": "GET",
        "collection": "thisButton",
        "property": "yRange",
        "variable": "yRange"
      },
      "var yMax = ${yRange} + ${yMin}",
      {
        "Note": "Determine the current y position.",
        "func": "GET",
        "collection": "thisButton",
        "property": "y",
        "variable": "ySlide"
      },
      {
        "Note": "If y goes above the maximum, set x to the maximum.",
        "func": "IF",
        "relation": ">",
        "thenRoutine": [
          {
            "func": "SET",
            "collection": "thisButton",
            "property": "y",
            "relation": "=",
            "value": "${yMax}"
          }
        ],
        "elseRoutine": [
          {
            "Note": "If y goes below the minimum, then set y to the minimum.",
            "func": "IF",
            "relation": "<",
            "thenRoutine": [
              {
                "func": "SET",
                "collection": "thisButton",
                "property": "y",
                "relation": "=",
                "value": "${yMin}"
              }
            ],
            "elseRoutine": [
              "var rotationVar = ${ySlide} - ${yMin} // If y is between the min and the max, calculate the value considering where its starting limit is.",
              {
                "func": "SET",
                "collection": "turnHolder",
                "property": "rotation",
                "relation": "=",
                "value": "${rotationVar}"
              },
              "var rotationVar = 360 - ${rotationVar} // Counterrotate the game pieces to left by the same amount the holder is turning to the right to keep the pieces face up.",
              {
                "func": "SET",
                "collection": "staticPiece",
                "property": "rotation",
                "relation": "=",
                "value": "${rotationVar}"
              }
            ],
            "operand1": "${ySlide}",
            "operand2": "${yMin}"
          }
        ],
        "operand1": "${ySlide}",
        "operand2": "${yMax}"
      }
    ],
    "text": "Y",
    "xLock": 1500,
    "yMin": 240,
    "yRange": 320
  },
