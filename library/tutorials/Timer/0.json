  "xen9": {
    "type": "timer",
    "id": "xen9",
    "x": 100,
    "y": 650,
    "z": 11,
    "changeRoutine": [
      "var Red = randInt 0 255",
      "var Green = randInt 0 255",
      "var Blue = randInt 0 255",
      "var newCSS = 'background\\u003a rgb(' + ${Red}",
      "var newCSS = ${newCSS} + ','",
      "var newCSS = ${newCSS} + ${Green}",
      "var newCSS = ${newCSS} + ','",
      "var newCSS = ${newCSS} + ${Blue}",
      "var newCSS = ${newCSS} + ')'",
      {
        "func": "SELECT",
        "property": "id",
        "value": "myHolder"
      },
      {
        "func": "SET",
        "property": "css",
        "value": "${newCSS}"
      }
    ],
    "precision": 500
  },
